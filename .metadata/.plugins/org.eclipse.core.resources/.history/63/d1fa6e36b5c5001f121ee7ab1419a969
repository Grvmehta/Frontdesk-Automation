{
    var autoUpdatePrice = document.getElementById(ctrl);
    YAHOO.util.Dom.setStyle(ctrl, "display","none");
    var autoUpdateLbl = document.getElementById(ctrlTxt);
    console.log(str1);
    console.log(str0);
    if(vl == 0)
    {
        autoUpdateLbl.innerHTML = str0;
    }
    else
    {
        autoUpdateLbl.innerHTML = str1;
    }
}

function toggleCaptionForGdsAttribute(ctrlTxt, ctrl, vl) {
	var autoUpdatePrice = document.getElementById(ctrl);
	YAHOO.util.Dom.setStyle(ctrl, "display","none");
	var autoUpdateLbl = document.getElementById(ctrlTxt);
	
	var domArr = ["No Meal Included",
				  "Continental Breakfast",
				  "Buffet BB",
				  "Halfboard",
				  "Fullboard"	
				 ];				 
	
	if(vl == 0)
		autoUpdateLbl.innerHTML = "No meals included in this package.";
	else
		autoUpdateLbl.innerHTML = "This Package includes "+domArr[vl];	
		

	updateOnAjax('gdsMealAttribute',ctrl,'','pck','','');
	return false;	
	
}

