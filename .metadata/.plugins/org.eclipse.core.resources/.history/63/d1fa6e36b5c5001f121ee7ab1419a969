<script type="text/javascript" src="https://qa1hz.stayez.com/styles/assets-hl/js/allotmentMatrix.js?v=20180721"></script>
<script language="javascript">
    
var pkgType = 'N';    
var seasonStartDateDom='2014-September-20,2014-December-01,2015-March-31';
var seasonStartDateDom=seasonStartDateDom.split(",");

var seasonEndDateDom='2014-December-01,2015-March-31,2015-June-30';
var seasonEndDateDom=seasonEndDateDom.split(",");

var seasonStartDateDomShort='2014-09-20,2014-12-01,2015-03-31';
var seasonStartDateDomShort=seasonStartDateDomShort.split(",");

var seasonEndDateDomShort='2014-12-01,2015-03-31,2015-06-30';
var seasonEndDateDomShort=seasonEndDateDomShort.split(",");

var clickToAdd = 'Click To Add';

var openCloseBoxFlag=0;

var pkgTempId = parseInt('9766');

var pkgPostingTypeMonthly = 'MONTHLY';
var gridType = '0';
var pkgRefType = 'fdesk';
var pkgRefTypeOth = 'oth';
var pkgStatus = 'A';
var isCentRate = '';
var isSyncToChannelAllow = '';
var syncOff = '0';

function remRadChk()
{
	var frm=document.getElementById('frmEditView');
	if(frm.validityTypeSA)
	{
		frm.validityTypeSA.checked=false;
		frm.validityTypeDT.checked=false;
	}	
}
function frmChk(frm,previewMode)
{
    if(openCloseBoxFlag>0)
    {
        alert("One or more of the properties are opened for editing,\n Please click on `OK` to close it first.");
        return false;
    }
        // check for minStay in form submiiting
        if(frm.minStay.value == '' || (parseFloat(frm.minStay.value) != parseInt(frm.minStay.value)) || isNaN(frm.minStay.value) || parseInt(frm.minStay.value) <= 0)
        {
            alert("Length Of Stay can not blank / should be integer only");
            return false;
        }
       
    if(frm.gdsTravelAgent)
    {
        if(frm.gdsTravelAgent.value == "")
        {
            alert("Please select travel agent for the package.");
            return false;
        }
    }
        var SyncDateSt = false;
	var dtValidityContClss=YAHOO.util.Dom.getStyle(document.getElementById('tabDT'), "display");
	var saValidityContClss=YAHOO.util.Dom.getStyle(document.getElementById('tabSA'), "display");
	if(pkgType == 'N' && (dtValidityContClss!='none' || saValidityContClss!='none'))
	{
		if(confirm("The changes have been made to the package. This action will discard the changes.\n Do you wish to continue?"))
		{
			   SyncDateSt = askForMaxSyncDate(frm,previewMode);
    
                            if(SyncDateSt)
                            {
                                disableSubmitButton();
                            }
                            else
                            {
                                return false;
                            }
		}else
		{
			return false;
		}
	}
	if(frm.packageProrated.value=="0" && frm.freeNight.value!=0)
	{
		alert("This Package is not Prorated.");
		return false;
	}
	if(gridType == '0' && pkgStatus !='A' && pkgRefType == pkgRefTypeOth)
	{
		alert("The inactive Third Party packages will not sync to Channel Manager/GDS.");	
	}
	if(gridType == '0' && '0' == '1')
	{
		alert("Some of the package(s) mapped to Channel Manager/GDS will not sync as they are not active.");	
	}
    
    SyncDateSt = askForMaxSyncDate(frm,previewMode);
    
    if(SyncDateSt)
    {
        if(previewMode == 'W' || previewMode == 'D')
        {
            return true;
        }
        else
        {
            disableSubmitButton();
        }
    }
    else
    {
        return false;
    }
    
}
function deleteRecOnAjax(id,rName,forWhat,vid) {
	
    var pkgId =0;
    if(arguments[4])
    {
        pkgId = arguments[4];
    }
	YAHOO.util.Dom.setStyle(rName+id,'background','#ff0000');
	if(forWhat=='posDisc')
		var alertText='Do you wish to delete this Pos Point?';
	if(forWhat=='canc')
		var alertText='Do you wish to delete this Cancellation Policy?';	
	if(forWhat=='book')
		var alertText='Do you wish to delete this Booking Policy?';		
	if(forWhat=='bulk')
		var alertText='Do you wish to delete this commission slab?';	
	if(forWhat=='pkgbookrest')
		var alertText='Do you wish to delete this restriction?';	
	if(forWhat=='ebd')
		var alertText='Do you wish to delete this Early Bird Discount?';	
	if(forWhat=='lmd')
		var alertText='Do you wish to delete this Last Minute Discount?';	
	if(forWhat=='lsd')
		var alertText='Do you wish to delete this Long Stay Discount?';	
	if(forWhat=='dpd')
		var alertText='Do you wish to delete this Dynamic Pricing?';	
				
	if(!confirm(alertText)) {
		YAHOO.util.Dom.setStyle(rName+id,'background','#ffffff');
		return false;
	}
	
	var handleSuccess = function(o){ 
		if(o.responseText !== undefined){ 
			YAHOO.util.Dom.setStyle(rName+id,'display','none');
			
			return false;
		} 
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	
	
	sUrl = 'https://qa1hz.stayez.com/admine/packagedetails/deletereconajax/id/'+id+"/pid/"+pkgId+"/vid/"+vid+"/forWhat/"+forWhat;
	var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback); 
	return false;
}

function enadleDisableOnAjax(type)
{
	if(type==0)
	{
		document.getElementById('btnSubmit').disabled=true;
        if(document.getElementById('btnRsSubmit'))
        {
            document.getElementById('btnRsSubmit').disabled=true;
        }
	}
	if(type==1)
	{
		document.getElementById('btnSubmit').disabled=false;
        if(document.getElementById('btnRsSubmit'))
        {
            document.getElementById('btnRsSubmit').disabled=false;
        }
	}
}
function toggleComm(idCh,validityId)
{
	if(idCh)
	{
		YAHOO.util.Dom.setStyle(document.getElementById('commTdId'+validityId),'display','');
		
	}else
	{		
		YAHOO.util.Dom.setStyle(document.getElementById('commTdId'+validityId),'display','none');
	}
}

function toggleCommForFdesk(idCh)
{
	if(idCh)
	{
		
		YAHOO.util.Dom.setStyle(document.getElementById('commTdId'),'display','');
		
	}else
	{	
		$('#fdPkgShowInAgent').prop('checked',false);
		YAHOO.util.Dom.setStyle(document.getElementById('commTdId'),'display','none');
	}
}

function toggleDisc(idCh,pkgId)
{
	var fdPkgShowInCorpObj  = '';
	
	if(document.getElementById('fdPkgShowInCorp'))
	{
		fdPkgShowInCorpObj = document.getElementById('fdPkgShowInCorp');
	}	
	
	if(idCh)
	{
		YAHOO.util.Dom.setStyle(document.getElementById('discTdId'),'display','');
		
	}else
	{		
		if(fdPkgShowInCorpObj!='')
		{
			fdPkgShowInCorpObj.checked=false;
			showInUpdateOnAjaxFromFdesk(pkgId,fdPkgShowInCorpObj,'corp');
		}	
		YAHOO.util.Dom.setStyle(document.getElementById('discTdId'),'display','none');
	}
}


function toggleFreeNight()
{

	var idCh = document.getElementById('freeNightTypeRn').checked;
	if(idCh)
	{
		YAHOO.util.Dom.setStyle(document.getElementById('freeNightRmPriceCon'),'display','');
		
	}else
	{		
		YAHOO.util.Dom.setStyle(document.getElementById('freeNightRmPriceCon'),'display','none');
		document.getElementById('freeNightRmPrice').checked=false;
		
	}
}

function commUpdateOnAjax(validityId)
{
	enadleDisableOnAjax(0);
	
	var comm = 0 ;	
	// var retrospective = 0 ;	
	
	if(document.getElementById('commissionable'+validityId).checked)	
		comm=document.getElementById('commissionable'+validityId).value;
	//if(document.getElementById('retrospective'+validityId).checked)	
		//retrospective=document.getElementById('retrospective'+validityId).value;	
		
	// #201404011400 #3658 grid changes
	var handleSuccess = function(o){ 
		if(o.responseText !== undefined){ 
			try { 
					
					enadleDisableOnAjax(1);
                    validityId = o.responseText;
                    window.location.href='https://qa1hz.stayez.com/admine/packagedetails/editview/id/9766/wiewvalidity/'+validityId;	
				
			}catch (x) { 
				alert("JSON Parse failed!"); 
				YAHOO.example.container.wait.hide();
				return; 
			}
		}
	
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	
	
	sUrl = 'https://qa1hz.stayez.com/admine/packagedetails/commupdate/pkgId/9766/id/'+validityId+'/comm/'+comm;
	var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback); 
}

function deleteValidity(pkgId,validityId)
{
	if(confirm("Do you really want to delete the selected validity?"))
	{
		var urlText1='https://qa1hz.stayez.com/admine/packagedetails/deletevalidity/pkgId/9766/oldPkgId/331223/validityId/'+validityId
		
		window.location.href=urlText1;
	}
}

function commUpdateOnAjaxFromFdesk(pkgId)
{
	enadleDisableOnAjax(0);
	
	var comm = 0 ;	
	
	var fdPkgShowInAgentObj  = document.getElementById('fdPkgShowInAgent');	
	
	if(document.getElementById('commissionable').checked)	
		comm = document.getElementById('commissionable').value;
		
	var handleSuccess = function(o){ 
		if(o.responseText !== undefined){ 
			try { 
					showInUpdateOnAjaxFromFdesk(pkgId,fdPkgShowInAgentObj,'agent');
					enadleDisableOnAjax(1);
				
			}catch (x) { 
				alert("JSON Parse failed!"); 
				YAHOO.example.container.wait.hide();
				return; 
			}
		}
	
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	
	
	sUrl = 'https://qa1hz.stayez.com/admine/packagedetails/commupdateforfdesk/pkgId/'+pkgId+'/comm/'+comm;	
	var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback); 
}


function showInUpdateOnAjaxFromFdesk(pkgId,obj,mode)
{
	enadleDisableOnAjax(0);
	
	var showInFlg = 0 ;	
		
	if(obj && obj.checked)	
		showInFlg = obj.value;
		
	var handleSuccess = function(o){ 
		if(o.responseText !== undefined){ 
			try { 
					
					enadleDisableOnAjax(1);
				
			}catch (x) { 
				alert("JSON Parse failed!"); 
				YAHOO.example.container.wait.hide();
				return; 
			}
		}
	
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	
	
	
	sUrl = 'https://qa1hz.stayez.com/admine/packagedetails/showinupdateforfdesk/pkgId/'+pkgId+'/showInFlg/'+showInFlg+"/mode/"+mode;	
	
	var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback); 
}



function inclPriceUpdateOnAjax(id,valueId,containerId,validityId)
{
	
	if(valueId == "") {
		return false;
	}
	
	var value=document.getElementById(valueId).value;
	
	var container=document.getElementById(containerId);
	if(container)
		container.innerHTML = 'Updating ...';
	
	var handleSuccess = function(o){ 
		if(o.responseText !== undefined){ 
			try { 
					
					var data =o.responseText;
					
					var price=data.split("|")[0];
					var totprice=data.split("|")[1];
					
					container.innerHTML =  price;
					
					//document.getElementById('totalPriceSpan').innerHTML =  totprice;
					
				
			}catch (x) { 
				alert("JSON Parse failed!"); 
				YAHOO.example.container.wait.hide();
				return; 
			}
		}
	
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	
	
	sUrl = 'https://qa1hz.stayez.com/admine/packagedetails/inclpriceupdate/id/'+id+'/value/'+value+'/pkgId/'+9766+'/validityId/'+validityId;
	var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback); 
}


function changeLifeTime()
{
	YAHOO.util.Dom.setStyle(document.getElementById("allTimeAnc"), "display","none");
	YAHOO.util.Dom.setStyle(document.getElementById("allTimeSpn"), "display","");
}

function editPkgForLifetime(type)
{
	var frm=document.getElementById('frmEditView');
	var fDate = '';
	var tDate = '';
	var seassionAtt = '';
	var seassionIndex = 0;
	if(type=='DT')
	{
		var fDate=frm.validFromDate.value;
		var tDate=frm.validToDate.value;
		
		var c1=new Date(getDateFromFormat(fDate,'yyyy-MM-d'));
		var c2=new Date(getDateFromFormat(tDate,'yyyy-MM-d'));
		if(fDate=='')
		{
			alert("Please select date.");
			frm.validFromDate.focus();
			return false;
		}
		if(tDate=='')
		{
			alert("Please select date.");
			frm.validToDate.focus();
			return false;
		}	
		if(!YAHOO.widget.DateMath.before(c1,c2))
		{
			alert("From date should be less than to date.");
			return false;
		}
	}else if(type=='SE'){
        seassionIndex= frm.seasonIndexDrop.value
		if(seassionIndex=='')
		{
			alert("Please select season.");
			return false;
		}
    }else
	{
		var seassionAtt= frm.seasonAttDrop.value
		if(seassionAtt=='')
		{
			alert("Please select season Attribute.");
			return false;
		}
    }
	 disableSubmitButton();
	var urlText1='https://qa1hz.stayez.com/admine/packagedetails/editpkgforlifetime/id/9766/startDate/';
	var mainUrl=urlText1+fDate+'/endDate/'+tDate+'/seassionAtt/'+seassionAtt+'/seassionIndex/'+seassionIndex;
	//alert(mainUrl);
	window.location.href=mainUrl;
}


function getPackagePrice(roomTypeId,pkgId,validityId,type) 
{
    var dontRunAjax = false;
    if(arguments[4] && arguments[4]==1)
    {
        dontRunAjax = true;
    }
    var changedPriceArgGiven = false;
    if(arguments[5])
    {
        changedPriceArgGiven = arguments[5];
    }
    var rmMonthlyRates=false;
    var showPackageRates=true;
    if(arguments[6])
    {
     rmMonthlyRates= arguments[6];  
     showPackageRates=false;
    }
    
    if(rmMonthlyRates && type==='pkgTotalsPn')
    {
        /** this is for monthly rates */
         var postingDays = parseInt(rmMonthlyRates.postingDaySlot);
         var stdOccupancy  = parseInt(rmMonthlyRates.stdOccupancy);
         var disBasePrice  = parseFloat(rmMonthlyRates.disBasePrice);
         var adults  = parseInt(rmMonthlyRates.adults);
         var children  = parseInt(rmMonthlyRates.children);
         var disUpChrgPrice  = parseFloat(rmMonthlyRates.disUpChrgPrice);
         var higherOccupancy  = parseFloat(rmMonthlyRates.higherOccupancy);
         //--------------------- TAX ------------------///
         var inclOfTax=0;
         var inclPriceWithoutTax  = parseFloat(rmMonthlyRates.inclPriceWithoutTax);
         var totalInclTax  = parseFloat(rmMonthlyRates.totalInclTax);
         var totalPkgFixTax  = parseFloat(rmMonthlyRates.totalPkgFixTax);
         var totalPkgPerTaxValue  = parseFloat(rmMonthlyRates.totalPkgPerTaxValue);
         var surChargeOnly  = parseFloat(rmMonthlyRates.surChargeOnly);
         //--------------------- END TAX ------------------///
         var totalPackagePriceBoxPrev  = parseFloat(rmMonthlyRates.totalPackagePriceBoxPrev); 
         var totalPackagePriceOriPrev  = parseFloat(rmMonthlyRates.totalPackagePriceOriPrev);
         var totalPackagePriceBox  = parseFloat(rmMonthlyRates.totalPackagePriceBox);
         
         var totalPackagePricePerNightBoxPrv  = parseFloat(rmMonthlyRates.totalPackagePricePerNightBoxPrev); 
         var totalPackagePricePerNightBoxOriPrv  = parseFloat(rmMonthlyRates.totalPackagePricePerNightBoxOriPrev);
    }
    else
    {
        /* this is old functionality for view edit */
         var postingDays = parseInt(document.getElementById("hidPostingDay_"+validityId).value);
         var stdOccupancy  = parseInt(document.getElementById("stdOccupancy"+roomTypeId+"_"+validityId).value);
         var disBasePrice  = parseFloat(document.getElementById("disBasePrice"+roomTypeId+"_"+validityId).value);
         var adults  = parseInt(document.getElementById("adultOpt"+roomTypeId+"_"+validityId).value);
         var children  = parseInt(document.getElementById("childrenOpt"+roomTypeId+"_"+validityId).value);
         var adultsPrev  = parseInt(document.getElementById("adultPrev"+roomTypeId+"_"+validityId).value);  
         var childrenPrev  = parseInt(document.getElementById("childrenPrev"+roomTypeId+"_"+validityId).value);
         var disUpChrgPrice  = parseFloat(document.getElementById("disUpChrgPrice"+roomTypeId+"_"+validityId).value);
         var higherOccupancy  = parseInt(document.getElementById("higherOccupancy"+roomTypeId+"_"+validityId).value);
         //--------------------- TAX ------------------///
         var inclOfTax  = document.getElementById("inclOfTax_"+validityId).checked;
         var inclPriceWithoutTax  = parseFloat(document.getElementById("inclPriceWithoutTax"+roomTypeId+"_"+validityId).value);
         var totalInclTax  = parseFloat(document.getElementById("totalInclTax"+roomTypeId+"_"+validityId).value); 
         var totalPkgFixTax  = parseFloat(document.getElementById("totalPkgFixTax"+roomTypeId+"_"+validityId).value);
         var totalPkgPerTaxValue  = parseFloat(document.getElementById("totalPkgPerTaxValue"+roomTypeId+"_"+validityId).value);
         var surChargeOnly  = parseFloat(document.getElementById("surChargeOnly"+roomTypeId+"_"+validityId).value);
         //--------------------- END TAX ------------------///
         var totalPackagePriceBoxPrev  = document.getElementById("totalPackagePriceBoxPrev_"+roomTypeId+"_"+validityId).value;
         var totalPackagePriceOriPrev  = parseFloat(document.getElementById("totalPackagePriceBoxPrevWOROF_"+roomTypeId+"_"+validityId).value);
         var totalPackagePriceBox  = parseFloat(document.getElementById("totalPackagePriceBox_"+roomTypeId+"_"+validityId).value);
         if(document.getElementById("totalPackagePricePerNightBoxPrev_"+roomTypeId+"_"+validityId))
         {
               var totalPackagePricePerNightBoxPrv  = parseFloat(document.getElementById("totalPackagePricePerNightBoxPrev_"+roomTypeId+"_"+validityId).value);
         }
         
         if(document.getElementById("totalPackagePricePerNightBoxPrevWOROF_"+roomTypeId+"_"+validityId))
         {
                var totalPackagePricePerNightBoxOriPrv  = parseFloat(document.getElementById("totalPackagePricePerNightBoxPrevWOROF_"+roomTypeId+"_"+validityId).value);
         }
    }
      
	var minStay  = parseFloat(document.getElementById("minStay").value);
	var extraPerson = ((adults + children) - stdOccupancy);
        var extraPersonCharge = 0;
	
        var withTaxFlag = 0;	
        var changedTotPkgPrice=totalPackagePriceOriPrev - totalPackagePriceBox;
	var changedTotPkgPriceOnRR=(disBasePrice*minStay) - changedTotPkgPrice;
     
	if(inclOfTax)
	{
		//--- calc for tax ---//
			withTaxFlag = 1;
			
			//changedTotPkgPrice=(totalPackagePriceBoxPrev - (inclPriceWithoutTax + totalInclTax)) - totalPackagePriceBox;
			
			//alert(totalPackagePriceBox+"========"+inclPriceWithoutTax+"======"+totalInclTax);
			//alert(totalPackagePriceBox)
			var newBoxValue = (totalPackagePriceBox - (inclPriceWithoutTax + totalInclTax));
			
			//newBoxValue = totalPackagePriceBox;
			//alert(newBoxValue+"========"+totalPkgFixTax+"======"+totalPkgPerTaxValue);
			var packgPriceTobeSet = (newBoxValue-totalPkgFixTax)/(1+(totalPkgPerTaxValue/100));
			
			//alert(packgPriceTobeSet);
			if(extraPerson>0)
			{
                //201508171400 #6829:sougata fix 
				extraPersonCharge = disUpChrgPrice*extraPerson*minStay;
			}else
			{
				extraPersonCharge=0;
			}
            
			packgPriceTobeSet = packgPriceTobeSet-extraPersonCharge-surChargeOnly;
			//alert(packgPriceTobeSet);
			var changedTotPkgPriceOnRR=packgPriceTobeSet;
		//--- calc for tax ---//
	}
	
	if(type==='pkgTotalsPn' || type==='pkgTotalsPnPostingDay')
	{
              var totalPackagePricePerNightBoxPrev  = totalPackagePricePerNightBoxPrv; 
              var totalPackagePricePerNightBoxOriPrev  = totalPackagePricePerNightBoxOriPrv;
         
        if(changedPriceArgGiven)
        {
            var totalPackagePricePerNightBox  =changedPriceArgGiven;
        }
        else
        {
            var totalPackagePricePerNightBox  = parseFloat(document.getElementById("totalPackagePricePerNightBox_"+roomTypeId+"_"+validityId).value);
        }
		
		var changedTotPkgPricePN=totalPackagePricePerNightBoxOriPrev - totalPackagePricePerNightBox;
                var changedTotPkgPriceOnRRPN=disBasePrice - changedTotPkgPricePN;
		changedTotPkgPriceOnRR = changedTotPkgPriceOnRRPN*minStay;
		
                if(inclOfTax)
		{
			//--- calc for tax ---//
				withTaxFlag = 1;
				
				
				
				//alert(totalPackagePricePerNightBox+"========"+inclPriceWithoutTax+"======"+totalInclTax);
				
				/*inclPriceWithoutTax = inclPriceWithoutTax/minStay;
				totalInclTax = totalInclTax/minStay;
				totalPkgFixTax = totalPkgFixTax/minStay;*/
				
				var newBoxValue = ((totalPackagePricePerNightBox*minStay) - (inclPriceWithoutTax + totalInclTax));
				
				//newBoxValue = newBoxValue/minStay;
				//newBoxValue = totalPackagePriceBox;
				//alert(newBoxValue);
				var packgPriceTobeSet = (newBoxValue-totalPkgFixTax)/(1+(totalPkgPerTaxValue/100));
				
				//alert(packgPriceTobeSet);
				if(extraPerson>0)
				{
					extraPersonCharge = disUpChrgPrice*extraPerson*minStay;
				}else
				{
					extraPersonCharge=0;
				}
				packgPriceTobeSet = packgPriceTobeSet-extraPersonCharge-surChargeOnly;
				var changedTotPkgPriceOnRR=packgPriceTobeSet;
				
				//alert(changedTotPkgPriceOnRR);
			//--- calc for tax ---//
		}
		
	}
	
	totalSel = parseInt(adults) + parseInt(children);

		if((changedTotPkgPriceOnRR<0) && type==='pkgTotals')
		{
			alert('Change not possible. Rooms price will become less than zero');
                        document.getElementById("totalPackagePrice_"+roomTypeId+"_"+validityId).innerHTML=totalPackagePriceBoxPrev;
			document.getElementById("totalPackagePriceBox_"+roomTypeId+"_"+validityId).value=totalPackagePriceBoxPrev;
			return false;
		}	
		else if((changedTotPkgPriceOnRRPN<0) && (type==='pkgTotalsPn' || type==='pkgTotalsPnPostingDay'))
		{
            if(dontRunAjax)
            {
                return "RPLESS";
            }
			alert('Change not possible. Rooms price will become less than zero');
			if(document.getElementById("totalPackagePricePerNight"+roomTypeId+"_"+validityId))
                        {
                           document.getElementById("totalPackagePricePerNight"+roomTypeId+"_"+validityId).innerHTML=totalPackagePricePerNightBoxPrev;
	                }
                        if(document.getElementById("totalPackagePricePerNightBox_"+roomTypeId+"_"+validityId))
                        {
                           document.getElementById("totalPackagePricePerNightBox_"+roomTypeId+"_"+validityId).value=totalPackagePricePerNightBoxPrev;
                        }
                   
            if(type==='pkgTotalsPnPostingDay' && document.getElementById("totalPackagePricePostingDay"+roomTypeId+"_"+validityId))
            {
                document.getElementById("totalPackagePricePostingDay"+roomTypeId+"_"+validityId).innerHTML=totalPackagePricePerNightBoxPrev*postingDays;
                document.getElementById("totalPackagePricePostingDayBox_"+roomTypeId+"_"+validityId).value=totalPackagePricePerNightBoxPrev*postingDays;
            }
            
            
			return false;
		}	
		else if(totalSel > higherOccupancy)
		{
			if(type==='Adult')
				document.getElementById("adultOpt"+roomTypeId+"_"+validityId).value = adultsPrev;
			else
				document.getElementById("childrenOpt"+roomTypeId+"_"+validityId).value = childrenPrev;
				
			alert("Maximum Occupancy is "+higherOccupancy);
			return false;		
		}
	
	if(dontRunAjax)
    {
         return parseFloat(changedTotPkgPriceOnRR);
    }
	var handleSuccess = function(o){
		if(o.responseText !== undefined){
			
                    if(showPackageRates)
                    {
                	document.getElementById("adultPrev"+roomTypeId+"_"+validityId).value=adults;
			document.getElementById("childrenPrev"+roomTypeId+"_"+validityId).value=children;
			
			var data = o.responseText;
			var pkgPrice=data.split('|')[0];
			var perNightPrice=data.split('|')[1];
			var bPrice=data.split('|')[2];
			
			var inclPriceWithoutTaxRt=data.split('|')[3];
			var totalInclTaxRt=data.split('|')[4];
			var totalPkgFixTaxRt=data.split('|')[5];
			var totalPkgPerTaxValueRt=data.split('|')[6];
			var surChargeOnly=data.split('|')[7];
			var pkgCurrConvertedPriceRate=data.split('|')[8];
                        var pkgPriceWOROF=data.split('|')[9];
                        var perNightPriceWOROF=data.split('|')[10];
		
			document.getElementById("disBasePrice"+roomTypeId+"_"+validityId).value = bPrice;
			
			document.getElementById("totalPackagePrice_"+roomTypeId+"_"+validityId).innerHTML = pkgPrice;
			
			document.getElementById("totalPackagePriceBox_"+roomTypeId+"_"+validityId).value =  pkgPrice;			
			
			document.getElementById("totalPackagePriceBoxPrev_"+roomTypeId+"_"+validityId).value =  pkgPrice;
            
			document.getElementById("totalPackagePriceBoxPrevWOROF_"+roomTypeId+"_"+validityId).value =  pkgPriceWOROF;
			
			document.getElementById("totalPackagePricePerNightBox_"+roomTypeId+"_"+validityId).value =  perNightPrice;			
			
			document.getElementById("totalPackagePricePerNightBoxPrev_"+roomTypeId+"_"+validityId).value =  perNightPrice;
            
			document.getElementById("totalPackagePricePerNightBoxPrevWOROF_"+roomTypeId+"_"+validityId).value =  perNightPriceWOROF;
			
			document.getElementById("totalPackagePricePerNight"+roomTypeId+"_"+validityId).innerHTML = perNightPrice;
            
            // POSTING DAY WISE
            if(document.getElementById("totalPackagePricePostingDay"+roomTypeId+"_"+validityId))
            {
                document.getElementById("totalPackagePricePostingDayBox_"+roomTypeId+"_"+validityId).value =  roundNumber(perNightPriceWOROF*postingDays);			
			
                document.getElementById("totalPackagePricePostingDayBoxPrev_"+roomTypeId+"_"+validityId).value =  roundNumber(perNightPriceWOROF*postingDays);

                document.getElementById("totalPackagePricePostingDayBoxPrevWOROF_"+roomTypeId+"_"+validityId).value =  perNightPriceWOROF*postingDays;

                document.getElementById("totalPackagePricePostingDay"+roomTypeId+"_"+validityId).innerHTML = roundNumber(perNightPriceWOROF*postingDays);
            }
            
            
			if(document.getElementById("othtd"+roomTypeId+"_"+validityId))
            {
                document.getElementById("othtd"+roomTypeId+"_"+validityId).innerHTML = pkgCurrConvertedPriceRate;
            }
			
			if(inclOfTax)
			{
			
				document.getElementById("inclPriceWithoutTax"+roomTypeId+"_"+validityId).value =  inclPriceWithoutTaxRt;	
				document.getElementById("totalInclTax"+roomTypeId+"_"+validityId).value =  totalInclTaxRt;	
				document.getElementById("totalPkgFixTax"+roomTypeId+"_"+validityId).value =  totalPkgFixTaxRt;	
				document.getElementById("totalPkgPerTaxValue"+roomTypeId+"_"+validityId).value =  totalPkgPerTaxValueRt;
				document.getElementById("surChargeOnly"+roomTypeId+"_"+validityId).value =  surChargeOnly;	
			
			}
                        
                    }
			
			
		}
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	};
	
	sUrl = 'https://qa1hz.stayez.com/admine/packagedetails/getpackageprice/';
        var pkgCurrId = '';
        var pkgCurrRate = '1';
	var postVar='roomTypeId='+roomTypeId+'&pkgId='+pkgId+'&validityId='+validityId+'&disBasePrice='+disBasePrice+'&stdOccupancy='+stdOccupancy+'&adults='+adults+'&children='+children+'&disUpChrgPrice='+disUpChrgPrice+'&minStay='+minStay+'&chnPkgP='+changedTotPkgPriceOnRR+'&type='+type+"&withTaxFlag="+withTaxFlag+"&oldDisBasePrice="+disBasePrice+"&pkgCurrId="+pkgCurrId+"&pkgCurrRate="+pkgCurrRate;
	
	var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback,postVar); 
}

function roundNumber(num,dec)
{
    if(!dec || dec==='')
    {
        dec = '2';
    }
    
    if(dec === '')
    {
        dec = 2;
    }
    
    dec = parseInt(dec);
    num = parseFloat(num);
	result = num.toFixed(dec); //USING JQUERY
	return result;
}

function getPackagePricePerPostingDay(roomTypeId,pkgId,validityId)
{
    var posTingDay = parseInt(document.getElementById("hidPostingDay_"+validityId).value);
    
    var totalPackagePricePostingDayBox  = parseFloat(document.getElementById("totalPackagePricePostingDayBox_"+roomTypeId+"_"+validityId).value);
     
    var newPrice = totalPackagePricePostingDayBox/posTingDay;
    
    var newPriceWD = newPrice;
    return getPackagePrice(roomTypeId,pkgId,validityId,'pkgTotalsPnPostingDay',0,newPriceWD) ;
}

function definePkgForDate(type)
{     
	var frm=document.getElementById('frmEditView');
	var fDate = '';
	var tDate = '';
	var seassionAtt = '';
	var isPeriodVal = 0;
	var copyRatesFromValidity = 0;
	

	if(type=='DT')
	{
		var fDate=frm.validFromDate.value;
		var tDate=frm.validToDate.value;
		var selFromSea=frm.seasonId.value;
		if(document.getElementById('copyRatesFromValidityDT'))
			copyRatesFromValidity=document.getElementById('copyRatesFromValidityDT').value;
			
		if(frm.isPeriod.checked)
			isPeriodVal = 1;
		var c1=new Date(getDateFromFormat(fDate,'yyyy-MM-d'));
		var c2=new Date(getDateFromFormat(tDate,'yyyy-MM-d'));
		
		if(fDate=='')
		{
			alert("Please select date.");
			frm.validFromDate.focus();
			return false;
		}
		if(tDate=='')
		{
			alert("Please select date. ");
			frm.validToDate.focus();
			return false;
		}	
		if(!YAHOO.widget.DateMath.before(c1,c2))
		{
			alert("From date should be less than to date. ");
			return false;
		}
	}
    else if(type=='SE')
	{
		seassionAtt= frm.seasonIndexDrop.value
		if(document.getElementById('copyRatesFromValiditySE'))
		{	
            copyRatesFromValidity=document.getElementById('copyRatesFromValiditySE').value;
        }
		if(seassionAtt=='')
		{
			alert("Please select season");
			return false;
		}
    }    
    else
	{
		var seassionAtt= frm.seasonAttDrop.value;
		if(document.getElementById('copyRatesFromValiditySA'))
			copyRatesFromValidity=document.getElementById('copyRatesFromValiditySA').value;
		if(seassionAtt=='')
		{
			alert("Please select season Attribute.");
			return false;
		}
    }
	disableSubmitButton();
	var urlText1='https://qa1hz.stayez.com/admine/packagedetails/defineratesnew/id/9766/';
	var mainUrl=urlText1+'rateType/'+type+'/startDate/'+fDate+'/endDate/'+tDate+'/selFromSea/'+selFromSea+'/rateTypeId/'+seassionAtt+'/isPeriod/'+isPeriodVal+'/copyRatesFromValidity/'+copyRatesFromValidity+"/validityType/"+type;
	//alert(mainUrl);
	window.location.href=mainUrl;
	
	
}


function closeOpenRate(id)
{
	var container=document.getElementById('ratesContainer_'+id);
	
	var clickAnc=document.getElementById('opnClAId_'+id);
	
	var sty = YAHOO.util.Dom.getStyle(container, "display");
	
	if(sty == "none") {
		
		YAHOO.util.Dom.setStyle(container, "display","");
		clickAnc.innerHTML="Hide";
		
	}else {
			YAHOO.util.Dom.setStyle(container, "display","none");
			clickAnc.innerHTML="View";
	}
	
	
} 

function closeOpenRateViewDetails(validityId, mainPkgId, tempPkgId)
{
	var container=document.getElementById('ratesContainer_'+validityId);
	
	var clickAnc=document.getElementById('opnClAId_'+validityId);
	
	var sty = YAHOO.util.Dom.getStyle(container, "display");
	
	if(sty == "none")
        {
		
            YAHOO.util.Dom.setStyle(container, "display","");
            clickAnc.innerHTML="Hide";
            var actionPost = 'https://qa1hz.stayez.com/admine/packagedetails/displaypkgvaliditydetails/id/'+mainPkgId;
            
            var loadingImgPath = "https://qa1hz.stayez.com/styles/assets-hl/css/wait.gif";
            var imgObj = document.createElement('img');
            var divObj = document.createElement('div');
            imgObj.setAttribute('src',loadingImgPath);
            divObj.setAttribute('style','line-height:25px;');
            divObj.innerHTML = 'Loading......';
            container.appendChild(imgObj);
            container.appendChild(divObj);

            var postData = {
                validityId: validityId,
                mainPkgId: mainPkgId,
                tempPkgId: tempPkgId
            };
            $.ajax({
                url: actionPost,
                type: "POST",
                dataType: "html",
                data: postData,
                crossDomain: false,            
                error: function (jqXHR, exception) {
                                 
                },
                success: function( data ) {
                   $(container).html(data);        
                }
            });
		
	}
        else
        {
            YAHOO.util.Dom.setStyle(container, "display","none");
            clickAnc.innerHTML="View";
	}
	
	
} 
/**
* 

 * @param {type} field
 * @param {type} valueId
 * @param {type} containerId
 * @param {type} updateTab
 * @param {type} txt
 * @param {type} position
 * @returns {Boolean} */
function updateOnAjax(field,valueId,containerId,updateTab,txt,position) {
	if(valueId == "") {
		return false;
	}
	
	
	var value=document.getElementById(valueId).value;
	var container=document.getElementById(containerId);
	if(value == "" && field=="pkgShortName")
	{
		alert("Rate Code cannot be blank.");		
		YAHOO.util.Dom.setStyle(document.getElementById(valueId),'display','');
		document.getElementById(valueId).value = document.getElementById('hdnActPkgStrNm').value;
		
		return false;
	}
	
	
	if(value == "" && field=="description")
	{
		alert("Package description can not be blank.");		
		YAHOO.util.Dom.setStyle(document.getElementById(valueId),'display','');
		document.getElementById(valueId).value = document.getElementById('descriptionHdn').value;
		
		return false;
	}
        // check for minLOS 
         if((value == "" ||(parseFloat(value) != parseInt(value)) || isNaN(value) || value <= 0 ) && field == "minStay")
        {
           alert("Length Of Stay can not blank / should be integer only");
           YAHOO.util.Dom.setStyle(document.getElementById(valueId),'display','');
           YAHOO.util.Dom.setStyle(document.getElementById(containerId),'display','none');
           return false;
        }
	
	
	
	
	if(container)
		container.innerHTML = 'Updating ...';
	
	var handleSuccess = function(o){ 
		if(o.responseText !== undefined){ 
			try { 
					if(container)
					{
                                               var data=o.responseText;
						var dup=data.split('~||~')[0];
						var dupVal=data.split('~||~')[1];
						if(dup=='DUP')
						{
							alert("Package Rate code should be unique");
							document.getElementById(valueId).value=dupVal;
							container.innerHTML = ''+dupVal+' '+txt+'';
						}else
						{
							if(position=='right')
								container.innerHTML = ''+value+' '+txt+'';
							else
								container.innerHTML = ''+txt+' '+value+'';
								
							if(field=="pkgShortName")
							{
								document.getElementById('hdnActPkgStrNm').value=value;
							}	
							if(field=="minStay")
							{   
                                                           document.getElementById('minStay').value = value;
							}	
						}
					}	
					
				
			}catch (x) { 
				alert("JSON Parse failed!"); 
				YAHOO.example.container.wait.hide();
				return; 
			}
		}
	
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	
	
	sUrl = 'https://qa1hz.stayez.com/admine/packagedetails/updateonajax';
	var postData = "id="+9766+"&field="+field+"&value="+encodeURIComponent(value)+"&updateTab="+updateTab;
	
	var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback, postData); 
}	
function openCloseBoxMode(mode)
{
    // mode=>1(Open);mode=>0(Closed)
    if(mode==1)
        openCloseBoxFlag = openCloseBoxFlag+1;
    else
       openCloseBoxFlag = openCloseBoxFlag-1;
}

function updateOnAjaxMulLang(field,valueId,containerId,updateTab,txt,position,langId,isDefault) {

	var hidpkgLAngTemiIdObj = '';
	var pkgLangRefId =0;
	if(document.getElementById('hidpkgLAngTemiId'+langId))
	{
		hidpkgLAngTemiIdObj = document.getElementById('hidpkgLAngTemiId'+langId);
		pkgLangRefId = hidpkgLAngTemiIdObj.value;
	}
	
	if(valueId == "") {
		return false;
	}
	
	
	var value=document.getElementById(valueId).value;
	var container=document.getElementById(containerId);
	if(value == "" && field=="pkgShortName")
	{
		alert("Rate Code cannot be blank.");		
		YAHOO.util.Dom.setStyle(document.getElementById(valueId),'display','');
		document.getElementById(valueId).value = document.getElementById('hdnActPkgStrNm').value;
		
		return false;
	}
	
	
	if((value == "" || value == clickToAdd) && field=="pkgName")
	{
		alert("Package name can not be blank.");		
		YAHOO.util.Dom.setStyle(document.getElementById(containerId),'display','none');
		YAHOO.util.Dom.setStyle(document.getElementById(valueId),'display','');
		
		document.getElementById(valueId).value = document.getElementById('hdnActPkgNm'+langId).value;
		
		return false;
	}
	if((value == "" || value == clickToAdd)  && field=="description")
	{
		alert("Package description can not be blank.");	
		YAHOO.util.Dom.setStyle(document.getElementById(containerId),'display','none');	
		YAHOO.util.Dom.setStyle(document.getElementById(valueId),'display','');
		document.getElementById(valueId).value = document.getElementById('descriptionHdn'+langId).value;
		
		return false;
	}
	
	
	
	
	if(container)
		container.innerHTML = 'Updating ...';

    if(field=="pkgName")
    {
         if(document.getElementById("autoTCont_"+langId))
         {
            document.getElementById("autoTCont_"+langId).style.display="none";
         }

    }
    if(field=="description")
    {
         if(document.getElementById("autoTDCont_"+langId))
         {
            document.getElementById("autoTDCont_"+langId).style.display="none";
         }

    }

	var handleSuccess = function(o){ 
		if(o.responseText !== undefined){ 
			try { 
					if(container)
					{
						var data=o.responseText;
						var dup=data.split('~||~')[0];
						var dupVal=data.split('~||~')[1];
						if(dup=='DUP')
						{
							alert("Package Rate code should be unique");
							document.getElementById(valueId).value=dupVal;
							container.innerHTML = ''+dupVal+' '+txt+'';
						}else
						{
							if(position=='right')
								container.innerHTML = ''+value+' '+txt+'';
							else
								container.innerHTML = ''+txt+' '+value+'';
							
							
							if(field=="pkgShortName")
							{
								document.getElementById('hdnActPkgStrNm').value=value;
							}	
							else if(field=="pkgName" || field=="description")
							{
								 hidpkgLAngTemiIdObj.value = data;
                                 }
								
						}
					}	
					
				
			}catch (x) { 
				alert("JSON Parse failed!"); 
				YAHOO.example.container.wait.hide();
				return; 
			}
		}
	
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	
	

	sUrl = 'https://qa1hz.stayez.com/admine/packagedetails/updateonajax';
	var postData = "id="+9766+"&field="+field+"&value="+encodeURIComponent(value)+"&updateTab="+updateTab+"&pkgLangRefId="+pkgLangRefId+"&langId="+langId+"&isDefault="+isDefault;
	
	var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback, postData); 
}
	
function ClearDate(vl) {	
	document.getElementById("displayFrom").value = "";
	document.getElementById("displayTill").value = "";
}
function getSeasonDates(sid) {
	
	var displayFrom=YAHOO.util.Dom.get("displayFrom");
	var displayTill=YAHOO.util.Dom.get("displayTill");
	var seasonId=YAHOO.util.Dom.get("seasonId");
	displayFrom.value="";
	displayTill.value="";
	si=0;
	if (seasonId.selectedIndex!=0) {
		si=seasonId.selectedIndex-1;
		displayFrom.value=seasonStartDateDomShort[si];
		displayTill.value=seasonEndDateDomShort[si];
		
	}
}

function handleSelect(type,args,obj) { 
	    var dates = args[0]; 
	    var date = dates[0]; 
	    var year = date[0], month = date[1], day = date[2]; 
 		
    	var txtDate1 = document.getElementById("displayFrom"); 
       	if (parseInt(day)<10) day='0'+day;
	   	if (parseInt(month)<10) month='0'+month;
    	txtDate1.value = year+"-"+month+"-"+day;   
    	
    	//txtDate1.value = convertIMySqlLocalDate_Format(day,month,year); 
    	document.getElementById("seasonId").value = "";
    	obj.hide();
}

function handleSelect2(type,args,obj) { 
	    var dates = args[0]; 
	    var date = dates[0]; 
	    var year = date[0], month = date[1], day = date[2]; 
 
    	var txtDate1 = document.getElementById("displayTill"); 
       	if (parseInt(day)<10) day='0'+day;
	   	if (parseInt(month)<10) month='0'+month;
    	txtDate1.value = year+"-"+month+"-"+day;   
    	//txtDate1.value =convertIMySqlLocalDate_Format(day,month,year); 
    	document.getElementById("seasonId").value = "";
    	obj.hide();
} 

function toggleValidity(v)
{
	if(v=="SA")
	{
		YAHOO.util.Dom.setStyle(document.getElementById('tabSA'),'display','');
		YAHOO.util.Dom.setStyle(document.getElementById('tabSE'),'display','none');
		YAHOO.util.Dom.setStyle(document.getElementById('tabDT'),'display','none');
	}
    else if(v=='DT')
	{
		YAHOO.util.Dom.setStyle(document.getElementById('tabSA'),'display','none');
		YAHOO.util.Dom.setStyle(document.getElementById('tabSE'),'display','none');
		YAHOO.util.Dom.setStyle(document.getElementById('tabDT'),'display','');
	}
	else{
		YAHOO.util.Dom.setStyle(document.getElementById('tabSA'),'display','none');
		YAHOO.util.Dom.setStyle(document.getElementById('tabDT'),'display','none');
		YAHOO.util.Dom.setStyle(document.getElementById('tabSE'),'display','');
	}
}

function changePostingRhythm(obj,pkgId)
{
    var postingType = obj.value;
   
    if(postingType === pkgPostingTypeMonthly)
    {
        var prevValue = obj.getAttribute("oldValue");
        obj.value = prevValue;
        obj.style.display="none";
        openMonthlyRatePanel();
        return false;
    }
    
    var handleSuccess = function(o){
		if(o.responseText !== undefined){
			if(o.responseText == pkgId)
            {
                window.location.href='https://qa1hz.stayez.com/admine/packagedetails/editview/id/'+pkgId;
            }else
            {
                alert("Error in change posting Type.");
            }
		}
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	};
        
    sUrl = 'https://qa1hz.stayez.com/admine/packagedetails/chnagepkgpostingrhythm/';
	var postVar='pkgId='+pkgId+'&postingType='+ postingType;
	
	var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback,postVar); 

}
var popupWindowMTHR=null;
function openMonthlyRatePanel()
{
    var w = 600;
    var h = 400;
    var left = (screen.width/2)-(w/2);
    var top = (screen.height/2)-(h/2);
    var url = 'https://qa1hz.stayez.com/admine/packagedetails/popupmonthly/pkgId/'+pkgTempId;
    popupWindowMTHR = window.open(url,'monthlyPopWin','width='+w+',height='+h+',top='+top+', left='+left+',scrollbars=1,resize=0');
    popupWindowMTHR.focus();
	return false;
   
}

function parent_disable() 
{
    if(popupWindowMTHR && !popupWindowMTHR.closed)
        popupWindowMTHR.focus();
}
function saveMonthlyRate(roomTypeId,pkgId,validityId,newPrice,monthlyRates)
{   
    return getPackagePrice(roomTypeId,pkgId,validityId,'pkgTotalsPn',1,newPrice,monthlyRates) ;
}
function toggleTextInc(box,label,act,id) {
	var selectOpt = document.getElementById(box);
	var labelOpt = document.getElementById(label);
	var box=document.getElementById('incItemPrice'+id);
	
	if(act == 'edit') {
	
		YAHOO.util.Dom.setStyle(selectOpt,'display','');
		box.focus();		
		YAHOO.util.Dom.setStyle(labelOpt,'display','none');
	}
	if(act == 'update') {
		box.value=labelOpt.innerHTML;
		YAHOO.util.Dom.setStyle(selectOpt,'display','none');
		YAHOO.util.Dom.setStyle(labelOpt,'display','');
	}
	return false;
	
}
function chnIncPrice(id,validitiId,genIds)
{
    var amt = document.getElementById("incItemPrice"+genIds).value;
    
	var urlText1='https://qa1hz.stayez.com/admine/packagedetails/inclpriceupdateonsubmit/id/';
	var mainUrl=urlText1+id+'/pkgId/'+9766+'/validitiId/'+validitiId+"/amt/"+amt;
	
	document.getElementById('frmEditView').action=mainUrl;
	document.getElementById('frmEditView').submit();
	//alert(mainUrl);
	//window.location.href=mainUrl;
}
function chnIncOfTax(obj,validitiId)
{
    var val = 0;
    if(obj.checked)
    {
        val = 1;
    }
        
	
	var urlText1='https://qa1hz.stayez.com/admine/packagedetails/incloftaxsubmit';
	var mainUrl=urlText1+'/pkgId/'+9766+'/validitiId/'+validitiId+"/val/"+val;
	
	document.getElementById('frmEditView').action=mainUrl;
	document.getElementById('frmEditView').submit();
	//alert(mainUrl);
	//window.location.href=mainUrl;
}
function toggleText(box,label,act,txt,position) {
	var selectOpt = document.getElementById(box);
	var labelOpt = document.getElementById(label);
	

	if(act == 'edit') {
		
		YAHOO.util.Dom.setStyle(selectOpt,'display','');
		
		selectOpt.focus();
			
		YAHOO.util.Dom.setStyle(labelOpt,'display','none');
	}
	if(act == 'update') {
		YAHOO.util.Dom.setStyle(selectOpt,'display','none');
		if(position=='right')
			labelOpt.innerHTML = ''+selectOpt.value+' '+txt+'';
		else
			labelOpt.innerHTML = ''+txt+' '+selectOpt.value+'';
				
		YAHOO.util.Dom.setStyle(labelOpt,'display','');
	}
	return false;
	
}

function showList(ctrl) {
	var sty = YAHOO.util.Dom.getStyle(ctrl, "display");
	
	if(sty == "none") {
		YAHOO.util.Dom.setStyle(ctrl, "display","");
	}else {
			YAHOO.util.Dom.setStyle(ctrl, "display","none");
	}
}
