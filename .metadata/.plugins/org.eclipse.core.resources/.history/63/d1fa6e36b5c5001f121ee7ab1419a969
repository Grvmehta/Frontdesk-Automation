{
    document.getElementById('frmEditView').doSaveAndReSync.value=1;
    if(frmChk(document.getElementById('frmEditView'),''))
    {
        document.getElementById('frmEditView').submit();
    }
}


$(document).ready(function() {    
     
        var allotMt = new allotmentMatrix();     
        allotMt.ready({
               refId : '-1',
               refType : 'fdesk',
               baseUrl : 'https://qa1hz.stayez.com',
               matrixStartDate : '',                              
               rateId : '',
               isReadOnlyAllotMatrix: ''
        }); 
        
        $(".link-ViewAllotGridInPkg").click(function(){           
            var dt = $(this).attr('date');
            var state = $(this).attr('state');
            
            if(state === 'open')
            {
                $(this).html('View');
                $("#dvAllotmentMatrix").html('');
                $("#dvAllotmentMatrix").hide();                
                $(this).removeAttr('state');
            }
            else
            {   
                $(this).html('Hide');
                $(this).attr('state', 'open');
                $("#dvAllotmentMatrix").show();
                allotMt.loadMatrixData(dt);   
                
            }
        });
        
        $(".link-editRestPkg").click(function(){           
            var dt = $(this).attr('date');
            window.open('https://qa1hz.stayez.com/admine/allotmentroom/listview/refId/-1/refType/fdesk/currDt/'+dt+'/agentId//rateId/');                        
            
        });
        

        $("#tooltipCon").tooltip();
        $('#tooltipCon').tooltip('option', 'position', { my: 'center bottom', at: 'center top-10',collision:'none',within:"table" });
        $('#tooltipCon').tooltip('option', 'tooltipClass', "top");        
 });
 
 function askForMaxSyncDate(frm,previewMode)
 {
    if(pkgType == 'N' && pkgStatus =='A' &&  isSyncToChannelAllow == '1' && syncOff != '1')
    {
        if(isCentRate === "1")
        {       
            if('0' === "1")
            {
                alert("Centralized rate Sync date is past current date.\n Please set the sync date from centralized rate.");
            }
            else if($("#maxSyncDate").val() == '' && ''!='')// for first time when child itself going for sync
            {
                alert(jsformatstr("Sync request will be sent upto %s1","Nov 30, -1"));
            }
            $("#maxSyncDate").val(''); // set parent max sync date.
            
            if($("#maxSyncDate").val() == '')
            {
                alert("Sync date is not setup. Please set the sync date from centralized rate.");
            }
            
            frm.clickPubToGrd.value = previewMode;
            frm.submit();
            
            return false;
        }
        
        var askBox = $("#askMaxSncDt");
        askBox.dialog({
                        modal:true
                        ,width:280
                        ,minHeight:29
                        ,create: function( event, ui ) {

                        }
                        ,open: function( event, ui ) {

                           // ui-dialog-titlebar ui-widget-header ui-corner-all ui-helper-clearfix ui-draggable-handle
                           $(".ui-dialog-titlebar").hide();
                           JQcalendar($("#maxSyncDateShow"));

                        },close: function( event, ui ) {
                            $("#maxSyncDateShow").val($("#maxSyncDateShow").attr("oldValue"))
                            $("#maxSyncDateShow").datepicker("destroy");
                        }
                        ,buttons:[ 
                                    { 
                                        id: "btnDlg-sync"
                                        ,text: "Save & Sync"
                                        ,click: function() 
                                        { 
                                            if($("#maxSyncDate").val() == '')
                                            {
                                                alert("Please select date.");
                                                return false;
                                            }
                                            $("#btnDlg-sync").html('<span class="ui-button-text">Syncing...</span>');
                                            $("#btnDlg-sync").attr('disabled', true);
                                            $("#btnDlg-close").attr('disabled', true);
                                            frm.clickPubToGrd.value = previewMode;
                                            frm.submit();
                                        }
                                    } 
                                    ,{ 
                                        id: "btnDlg-close"
                                        ,text: "Cancel"
                                        ,click: function() 
                                        { 
                                            $( this ).dialog( "close" ); 
                                        }
                                    } 
                                  ]
                      }).dialog('open');

            return false;
    }
    else
    {
    
      frm.clickPubToGrd.value = previewMode;
      return true;
    }
                
                
 }
 

 
function JQDatepickerOnselect(eleObj,selected,onselectCallBack)
{
    if (onselectCallBack && typeof onselectCallBack == 'function')            { 
                onselectCallBack(); 
    }

    if (eleObj.hasClass("fromDate"))
    {
        var minDate = new Date(selected);
        if (minDate) {
            if(eleObj.hasClass("sameDate"))
            {
                minDate.setDate(minDate.getDate());
            }else
            {
                minDate.setDate(minDate.getDate() + 1);
            }
        }
        eleObj.closest("tr").find(".toDate").datepicker("option", "minDate", minDate);
    }
    else if (eleObj.hasClass("toDate"))
    {
        var maxDate = new Date(selected);
        if (maxDate) {
            if(eleObj.hasClass("sameDate"))
            {
               maxDate.setDate(maxDate.getDate());
            }else
            {
                maxDate.setDate(maxDate.getDate() - 1);
            }
            
            
        }
        //eleObj.closest("tr").find(".fromDate").datepicker("option", "maxDate", maxDate);

    }
}

function JQcalendar(eleObj,onselectCallBack)
{
    var options = {
        dateFormat: eleObj.attr('dateFormat')?eleObj.attr('dateFormat'):"yy-mm-dd",
        changeMonth: true,
        changeYear: true,
        minDate: eleObj.attr('minDate'), 
        maxDate: eleObj.attr('maxDate'),
        onSelect: function(selected) {

            JQDatepickerOnselect(eleObj,selected,onselectCallBack);
        }
        
    };
    
    eleObj.datepicker(options);
   
    if(eleObj.attr('useAltField') && eleObj.attr('useAltField').length>0)
    {
        eleObj.datepicker( 'option' , 'altField', "#"+eleObj.attr('useAltField') );
        eleObj.datepicker( 'option' , 'altFormat', "yy-mm-dd" );
    }
    
    if (!eleObj.hasClass("dontShowIcon"))
    {
       eleObj.datepicker( 'option' , 'showOn', "button" );
       eleObj.datepicker( 'option' , 'buttonImage', "https://qa1hz.stayez.com/images/calendar.gif" );
       eleObj.datepicker( 'option' , 'buttonImageOnly', true );
    }
}

/**
 * function is called when day use checkbox changes
 * @param {type} obj
 * @param {type} pkgId
 * @returns {undefined} 
 */
function dayUseChange(obj,pkgId)
{
    var isDayUse = (obj.checked) ? 1:0;
    var frm = document.getElementById('frmEditView');
    if(frm.minStay && frm.minStay.value > 1 && obj.checked == true)
    {
        alert("Package can not be marked as Day Use.");
        obj.checked = false;
        return;
    }
    
    var handleSuccess = function(o){
            if(o.responseText !== undefined)
            {
                if(o.responseText == pkgId)
                {
                    window.location.href='https://qa1hz.stayez.com/admine/packagedetails/editview/id/'+pkgId;
                }else
                {
                    alert("Error in Day Use.");
                }
            }
	}
	var handleFailure = function(o){ 
            if(o.responseText !== undefined){ 
                    return "Operation Failed";
            } 
	}
	var callback = { 
            success:handleSuccess, 
            failure: handleFailure, 
            argument: { foo:"foo", bar:"bar"} 
	};
        
    sUrl = 'https://qa1hz.stayez.com/admine/packagedetails/changedayuse/';
	var postVar='pkgId='+pkgId+'&isDayUse='+ isDayUse;
	var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback,postVar); 
}
</script>

    <script language="javascript">
function doBack(frm) {
    
    var ac = "https://qa1hz.stayez.com/admine/packagedetails/listview";
    if(arguments[1] && arguments[1]!='')
    {
        ac = arguments[1];
    }
	frm.action = ac;
	frm.submit();
	
}
function fillData(act,vl,targetContainer,selectedValue) {

    var addSelOp = 0;

    if(arguments[4])
    {
        addSelOp = arguments[4];
    }
    
	var handleSuccess = function(o){ 
		if(o.responseText !== undefined){ 
			var messages = []; 
			
			var listObj = document.getElementById(targetContainer);
			
			listObj.length = 1;
			
	         try { 
     	       messages = YAHOO.lang.JSON.parse(o.responseText);

               if(messages==null)
               {
                return false;
               }
        	} catch (x) { 
	            alert("JSON Parse failed!"); 
	            return; 
	        }
	        
	        //Append Default
	       		var refChild = YAHOO.util.Dom.getChildren(listObj);
	        for (var i = 0, len = messages.length; i < len; ++i) { 
		         var m = messages[i];
		         var refChild = document.createElement('OPTION');
		         var refChild = new Option(m.field);
		         
 		         refChild.value = m.id;
 		         refChild.text = m.field;
 		         
 		         if(parseInt(selectedValue) == parseInt(m.id)) {
	 		         refChild.selected = true;
 		         }
                 if(addSelOp==1)
                 {
                    listObj[i] = refChild;
                 }else
                 {
                    listObj[i+1] = refChild;
                 }
	        }
		} 
	}
	

	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	

	sUrl = 'https://qa1hz.stayez.com/admine/ajax/'+act+'/id/'+vl;
	var request = YAHOO.util.Connect.asyncRequest('GET', sUrl, callback); 
	
}

function fillPosProAppTax(act,val,targetContainer,cnt, isInclusive)
{
	var listObj = document.getElementById(targetContainer);
	listObj.innerHTML='loading..';
	document.getElementById('btnSave').disabled=true; 
	var handleSuccess = function(o){ 
		if(o.responseText !== undefined){ 
			
			
	         try { 
     	       messages =o.responseText; 
			   listObj.innerHTML='';
			   listObj.innerHTML=messages;
			   document.getElementById('btnSave').disabled=false; 
        	} catch (x) { 
				 document.getElementById('btnSave').disabled=false; 
	            alert("JSON Parse failed!"); 
	            return; 
	        }
	        
	       
		} 
	}
	

	var handleFailure = function(o){ 
		 document.getElementById('btnSave').disabled=false; 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	

	sUrl = 'https://qa1hz.stayez.com/admine/ajax/'+act+'/posPointId/'+val+'/cnt/'+cnt+'/isinc/'+isInclusive;
	var request = YAHOO.util.Connect.asyncRequest('GET', sUrl, callback); 
}

function MM_goToURL() { //v3.0
  var i, args=MM_goToURL.arguments; document.MM_returnValue = false;
  for (i=0; i<(args.length-1); i+=2) eval(args[i]+".location='"+args[i+1]+"'");
}

function MM_openBrWindow(url,name,param) {
	window.open(url,name,param);
	return false;
}

function MM_openSurWindow(url,name,param) {
	window.open(url,name,param);
	return false;
}

function deleteImage(tableName,id,container,type,refId) {
	
	
	YAHOO.util.Dom.setStyle(container,'background','#ff0000');
	                
	if(!confirm('Do you really want to delete selected image.')) {
		YAHOO.util.Dom.setStyle(container,'background','#ffffff');
		return false;
	}
	
	
	var handleSuccess = function(o){ 
		if(o.responseText == 1) {
			YAHOO.util.Dom.setStyle(container,'display','none');
		}
		if(o.responseText == 0) {
			alert("Image could not Delete!");
		}
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	

	sUrl = 'https://qa1hz.stayez.com/admine/image/imagedelete/tablename/'+tableName+'/id/'+id+'/type/'+type+'/refid/'+refId;
	var request = YAHOO.util.Connect.asyncRequest('GET', sUrl, callback); 
	return false;

}

function deleteWebSettingPdf(tableName,colName,fieldUpdate,id,container) {
	
	
	YAHOO.util.Dom.setStyle(container,'background','#ff0000');
	
	if(!confirm('Do you Realy Want to delete select Pdf')) {
		YAHOO.util.Dom.setStyle(container,'background','#ffffff');
		return false;
	}
	
	
	var handleSuccess = function(o){ 
		if(o.responseText == 1) {
			YAHOO.util.Dom.setStyle(container,'display','none');
			YAHOO.util.Dom.get(fieldUpdate).value='';
			
		}
		if(o.responseText == 0) {
			alert("Image could not Delete!");
		}
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	

	sUrl = 'https://qa1hz.stayez.com/admine/image/webpdfdelete/tablename/'+tableName+'/colName/'+colName+'/id/'+id;
	var request = YAHOO.util.Connect.asyncRequest('GET', sUrl, callback); 
	return false;

}


function toggleTextImageCaption(box,label,act,txt,position) {
	var selectOpt = document.getElementById(box);
	var labelOpt = document.getElementById(label);
	
	
	if(act == 'edit') {
		
		YAHOO.util.Dom.setStyle(selectOpt,'display','');
		
		selectOpt.focus();
			
		YAHOO.util.Dom.setStyle(labelOpt,'display','none');
	}
	if(act == 'update') {
		YAHOO.util.Dom.setStyle(selectOpt,'display','none');
		if(position=='right')
			labelOpt.innerHTML = ''+selectOpt.value+' '+txt+'';
		else
			labelOpt.innerHTML = ''+txt+' '+selectOpt.value+'';
				
		YAHOO.util.Dom.setStyle(labelOpt,'display','');
	}
	return false;
	
}



	function removeLogoImage(container, logoHiddenField){
		YAHOO.util.Dom.setStyle(container,'background','#ff0000');
		
		if(!confirm('Do you Realy Want to delete the logo.') ) {
			YAHOO.util.Dom.setStyle(container,'background','#ffffff');
			return false;
		}
		YAHOO.util.Dom.setStyle(container,'display','none');
		YAHOO.util.Dom.get(logoHiddenField).value="";
	}

	var weekday=new Array(7);
	weekday[0]='Sunday';
	weekday[1]='Monday';
	weekday[2]='Tuesday';
	weekday[3]='Wednesday';
	weekday[4]='Thursday';
	weekday[5]='Friday';
	weekday[6]='Saturday';
	
	
	var monthsName = [];
	monthsName[1] = "January";
	monthsName[2] = "February";
	monthsName[3] = "March";
	monthsName[4] = "April";
	monthsName[5] = "May";
	monthsName[6] = "June";
	monthsName[7] = "July";
	monthsName[8] = "August";
	monthsName[9] = "September";
	monthsName[10] = "October";
	monthsName[11] = "November";
	monthsName[12] = "December";
	
	var monthsNameShort = [];
	monthsNameShort[1] = "Jan";
	monthsNameShort[2] = "Feb";
	monthsNameShort[3] = "Mar";
	monthsNameShort[4] = "Apr";
	monthsNameShort[5] = "May";
	monthsNameShort[6] = "Jun";
	monthsNameShort[7] = "Jul";
	monthsNameShort[8] = "Aug";
	monthsNameShort[9] = "Sep";
	monthsNameShort[10] = "Oct";
	monthsNameShort[11] = "Nov";
	monthsNameShort[12] = "Dec";	
	
	
	
	function convertDateFromMysqlToLocaleFormat(day,month,year) {
	
		var LOCALE_DATE_FORMAT="short";
 		var dat=day+'/'+month+'/'+year;
 		var d = new Date();
	 		d.setDate(day);
	 		d.setMonth(month-1);
			d.setYear(year);

		var dayOfDate=d.getDay();
 		var dayNameOfDate=weekday[dayOfDate];
 		
 		var monthName=monthsName[month];
 		
		var retDate = "";
		
 		if (LOCALE_DATE_FORMAT=='full') //  Wednesday, November 26, 2008
 			retDate = dayNameOfDate + ', ' + monthName + ' ' + day + ', ' + year;
 		if (LOCALE_DATE_FORMAT=='long')  //  November 26, 2008
			retDate = monthName + ' ' + day + ', ' + year;
 		if (LOCALE_DATE_FORMAT=='medium')  //  Nov 26, 2008
			retDate = monthsNameShort[month] + ' ' + day + ', ' + year;
 		if (LOCALE_DATE_FORMAT=='short')  //  01/26/08
			retDate = month + '/' + day + '/' + year;
 			
			
 		return retDate;
		
			
	}
	
	
	
	function changProductInInclution(posIdObj) {
	
		var posId=posIdObj.value;
		document.getElementById('prodIdMannual').disabled=true;
		var handleSuccess = function(o){
			if(o.responseText !== undefined){
				document.getElementById('posProListId').innerHTML=o.responseText;
				document.getElementById('prodIdMannual').disabled=false;
				
			}
		}
		
		var handleFailure = function(o){
			document.getElementById('prodIdMannual').disabled=false;
			if(o.responseText !== undefined){
				return "Operation Failed";
			}
		}
		var callback={
			success:handleSuccess,
			failure: handleFailure,
			argument: { foo:"foo", bar:"bar" }
		};
		
		sUrl = 'https://qa1hz.stayez.com/admine/packagedetails/changproduct/?posPointId='+posId;
		
		var request = YAHOO.util.Connect.asyncRequest('GET', sUrl, callback);
	}

	
	
	
