function getPackageOtherCurrencyPrice(roomTypeId,pkgId,validityId,obj) 
{
	
	
	var totalPackagePriceBoxPrev  = parseFloat(document.getElementById("totalPackagePriceOthCurrBoxPrev_"+roomTypeId+"_"+validityId).value);	
	var totalPackagePriceBox  = parseFloat(document.getElementById("totalPackagePriceOthCurrBox_"+roomTypeId+"_"+validityId).value);
    var totalPackageOthCurrPrice = parseFloat(obj.value);
    var currencyId = document.getElementById("pkgCurrency").value;
    var otherCurrSpanObj = document.getElementById("totalPackagePriceOthCurrSpan"+roomTypeId+"_"+validityId);
	
    
    
    if(currencyId == 0)
    {
        document.getElementById("totalPackagePriceOthCurrBox_"+roomTypeId+"_"+validityId).value = totalPackagePriceBoxPrev;
        
        alert("Plesae select package currency first!");
        
        return(false);
    }
	var convertionRate = totalPackageOthCurrPrice / totalPackagePriceBox ;
	var handleSuccess = function(o){
		if(o.responseText !== undefined){
			
			var data = o.responseText;
			var otherCurrPrice =data;
			
			document.getElementById("totalPackagePriceOthCurrBoxPrev_"+roomTypeId+"_"+validityId).value =  otherCurrPrice;			
			document.getElementById("totalPackagePriceOthCurrBox_"+roomTypeId+"_"+validityId).value =  otherCurrPrice;			
			document.getElementById("totalPackagePriceOthCurr"+roomTypeId+"_"+validityId).innerHTML =  otherCurrPrice;
			
			
		}
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	};
	
	sUrl = 'https://qa1hz.stayez.com/admine/packagedetails/updatepkgothercurrencyprice/';
	var postVar='roomTypeId='+roomTypeId+'&pkgId='+pkgId+'&validityId='+validityId;
    postVar +=  '&convertionRate='+convertionRate+'&amount='+totalPackageOthCurrPrice;
    postVar += '&currencyId='+ currencyId;
	
	var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback,postVar); 
}

function pkgcurrencychanged(obj,pkgId,validityId,convRate)
{
    if(obj != null)
    {
        var pkgcurrid = parseInt(obj.value);
        var currshortname = '';
        if(pkgcurrid > 0 )
        {    
            currshortname = obj.options[obj.selectedIndex].getAttribute('shortcode');
        }
    }
    else
    {
        var pkgcurrid = parseInt('');
        var currshortname = '';
    }
    
    
    var handleSuccess = function(o){
		if(o.responseText !== undefined){
			
			var data = o.responseText;
                        
                        if(data > 0)
                        {
                            document.getElementById("changeConvRate").style.display="block";
                            document.getElementById("changeConversionRate").value = data;
                            
                        }
                        else
                        {
                            alert("Conversion Rate is not found for this currency!");
                            document.getElementById("changeConvRate").style.display="block";
                            document.getElementById("changeConversionRate").value = 1;    
                        }
			var validity = '';
                        if(validityId)
                        {
                            var validity = '/wiewvalidity/'+validityId;
                        }
                        window.location.href='https://qa1hz.stayez.com/admine/packagedetails/editview/id/'+pkgId+validity;
		}
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	};
        
    sUrl = 'https://qa1hz.stayez.com/admine/packagedetails/getconversionrate/';
	var postVar='pkgId='+pkgId+'&pkgcurrencyId='+ pkgcurrid+'&pkgcurrcode='+currshortname;
	if(convRate)
        {
           postVar+='&conversionRate='+convRate;  
        }
	var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback,postVar); 
}

function openMonthlyAddValidityPanel()
{
    var w = 800;
    var h = 300;
    var left = (screen.width/2)-(w/2);
    var top = (screen.height/2)-(h/2);
    var url = 'https://qa1hz.stayez.com/admine/packagedetails/addvaliditytomonthlymode/pkgId/'+pkgTempId;
    popupWindowMTHR = window.open(url,'monthlyPopWin','width='+w+',height='+h+',top='+top+', left='+left+',scrollbars=1,resize=0');
    popupWindowMTHR.focus();
	return false;
   
}

function clickAddValidity(pkgPostingType,callBack)
{
    if(pkgPostingType === pkgPostingTypeMonthly)
    {
        openMonthlyAddValidityPanel();
    }else
    {
        callBack('tabDT','allTimeAnc','edit','','');
    }
}

