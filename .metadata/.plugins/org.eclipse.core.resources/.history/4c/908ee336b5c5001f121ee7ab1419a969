function loadGridData()
{
    var jsonDataStr = '[{"name":"actions","label":"Actions","width":50,"formatter":"actions","formatoptions":{"keys":true,"editOptions":[],"delOptions":[]},"classes":"lightBlue","frozen":true},{"label":"Error Messages","name":"message","width":100,"classes":"lightRed","frozen":true},{"label":"Ref Id","name":"refid","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Check In","name":"checkInDate","width":80,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Check Out","name":"checkOutDate","width":80,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Room Type","name":"roomTypeCode","width":80,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Pkg","name":"rateCode","width":80,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Rate","name":"rate","width":80,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Adults","name":"noOfAdult","width":50,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Child","name":"noOfChild","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Status","name":"rsvStatus","width":100,"classes":"invalidGrid","editable":true,"edittype":"select","editoptions":{"value":":Select;R:R;B:B;HT:HT;C:C"}},{"label":"Res Owner First Name","name":"ownerFName","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Res Owner Last Name","name":"ownerLName","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Res Owner Phone","name":"ownerPhone","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Res Owner Mobile No.","name":"ownerMobile","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Res Owner Email","name":"ownerEmail","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Res Owner Address","name":"ownerAddress","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Res Owner City","name":"ownerCity","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Res Owner State","name":"ownerState","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Res Owner Country","name":"ownerCountry","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Res Owner Zip","name":"ownerZip","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Res Owner Gender","name":"ownerGender","width":100,"classes":"invalidGrid","editable":true,"edittype":"select","editoptions":{"value":":Select;M:M;F:F"}},{"label":"Grp Owner Type","name":"grpOwnerType","width":100,"classes":"invalidGrid","editable":true,"edittype":"select","editoptions":{"value":":Select;A:A;C:C;G:G"}},{"label":"Company Name","name":"compName","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Grp Owner First Name","name":"grpOwnerFName","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Grp Owner Last Name","name":"grpOwnerLName","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Grp Owner Phone Number","name":"grpOwnerPhone","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Grp Owner Mobile Number","name":"grpOwnerMobile","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Grp Owner Email","name":"grpOwnerEmail","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Grp Owner Address","name":"grpOwnerAddress","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Grp Owner City","name":"grpOwnerCity","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Grp Owner State","name":"grpOwnerState","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Grp Owner Country","name":"grpOwnerCountry","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Grp Owner Zip","name":"grpOwnerZip","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Grp Owner Gender","name":"grpOwnerGender","width":100,"classes":"invalidGrid","editable":true,"edittype":"select","editoptions":{"value":":Select;M:M;F:F"}},{"label":"Comments","name":"comments","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Payment Mode","name":"paymentMode","width":100,"classes":"invalidGrid","editable":true,"edittype":"select","editoptions":{"value":":Select;cash:cash;cc:cc;cheque:cheque;bank:bank;oth:oth"}},{"label":"Payment Type","name":"paymentType","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Amount","name":"paymentAmount","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"paymentDesc","name":"paymentDesc","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Cancel\/Hold Till Date","name":"cancelOrHoldTillDate","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Cancellation Charges","name":"cancelCharge","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"},{"label":"Reason of Cancellation","name":"cancelRemarks","width":100,"classes":"invalidGrid","editable":true,"edittype":"text"}]';
    var jsonRecordPerPageList = '[20,30,40,50,60]';
    if(jsonDataStr != '')
    {
        var jqGridElem = $("#jqGrid");
        var jqGridPagerElem = $("#jqGridPager");
       
        var jsonData = $.parseJSON(jsonDataStr);

        var type = $("#recordType").val(); 
        var viewUrl = 'https://staygrid.com/hmsadmine/importrsv/loadcsvdata/hotelId/16164/type/'+type;
        var editUrl = 'https://staygrid.com/hmsadmine/importrsv/savecsvdata/hotelId/16164/type/'+type;

        var gridObj = new jQueryGridI();
        gridObj.init(jsonData, viewUrl, editUrl, jqGridElem, jqGridPagerElem, $("#totalRecords"));
        gridObj.rowNum = '20';
        gridObj.rowList = $.parseJSON(jsonRecordPerPageList);
        gridObj.setdatepicker('checkInDate');
        gridObj.setdatepicker('checkOutDate');
        gridObj.showGrid();
        gridObj.afterInsertRow = changeGridRowAttr;
        $(gridObj.gridElement).on('jqGridInlineAfterSaveRow', function(data) {
            var counter = 0;
            $.each($("#jqGrid_frozen div[id*=jSaveButton_]"), function (index, value){
                if($(this).css('display') != 'none')
                {
                    counter++;
                }
            });
            if(counter == 1)
            {
                gridObj.isRefresh = true;
            }
             if(gridObj.isRefresh)
            {
                $(this).trigger('reloadGrid');
            }
        });
        
        gridObj.feezeCols();
    }
}

function removeSelectionForAllGdsRm(gdsId)
{
    $.each($("input[name*=rmTypeId]"), function (index, value){
       var gds = $(this).attr('gds');
       if(gds != gdsId)
       {
           $(this).prop("checked",false);
       }
    });
}
$(document).ready(function () {



$('#file').on('change', function (event){
    files = event.target.files;
});

$('form').on('submit', function (event) {

     event.stopPropagation(); 
     event.preventDefault(); 
     $("#submit").prop('disabled',true);
    if(files && files.length > 0)
    {
        var data = new FormData();
        $.each(files, function(key, value)
        {
            data.append(key, value);
        }); 

        var action = "https://staygrid.com/hmsadmine/importrsv/uploadcsv/hotelId/16164";
        $("#loader").show();
        ajaxSubmit(action,data,successupload,"POST","json",true,$("#loader"));
    }
    else
    {
        $("#submit").prop('disabled',false);
        $("#error").html("Please upload a csv file!");
    }
});


$("#validationAll").on('click', function (event){
    var callerObj = $("#validationAll");
    var actionLabel = "Validation";
    var progressBarDObj = $("#progressbar");
    var progressLabelObj = $(".progress-label");
    var dialogObj = $("#dialog");
    var isModal = true;
    var validationLimit = '50';
    var totalRecords = $("#totalRecords").val();
    var totalSteps = Math.ceil(totalRecords/validationLimit);
    var type = 'INVALID';
    
    var pgBar = new jqueryProgressBar();
    pgBar.init(callerObj,actionLabel,progressBarDObj,progressLabelObj,dialogObj,isModal);
    
    validateAjaxCall(1, totalSteps, validationLimit, totalRecords, type);
});  

