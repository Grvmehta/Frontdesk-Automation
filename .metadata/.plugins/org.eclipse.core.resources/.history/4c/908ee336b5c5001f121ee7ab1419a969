<script language="javascript">

var files;
var gridObj;
var totalSteps;


function validationcomplete(data)
{
        if(data.error == 0)
    {
        if(data.complete == 0)
        {
            validateAjaxCall(data.step, data.totalsteps, data.limit, data.total, data.type);
        }
        else
        {
            $("#recordType").val('INVALID');
            jqueryProgressBar.progressbar.ajaxStopLoading();
            window.location.href= 'https://staygrid.com/hmsadmine/importrsv/showcsvdata/hotelId/16164/cd/i'
        }
    }
    else
    {
        jqueryProgressBar.progressbar.ajaxStopLoading();
        var messages = "<ul>";
        if($.isPlainObject(data.errorMessage))
        {
            $.each(data.errorMessage, function (index, value){
                messages += "<li>" + value + "</li>";
            });
        }
        else
        {
           messages +=  "<li>" + data.errorMessage + "</li>";
        }
        
        messages += "</ul>";
        $("#error").html(messages);
    }
}


function validateAjaxCall(step,totalSteps,limit,total,type)
{
    var fromRecord = (step*limit) - limit;
    var toRecord = (step*limit) - 1;
    var allTotal = $("#totalRecords").val();
    var compRec = parseInt(allTotal - total);
    var percent = Math.floor((compRec/allTotal)*100);
    $("#progressText").html("<br><br><strong>Records Remaining:</strong> "+total);
    jqueryProgressBar.progressbar.ajaxStartLoading(true,totalSteps,step,percent);
    
    var action = 'https://staygrid.com/hmsadmine/importrsv/validationcsvdata/step/'+step;
    var postData = {
            hotelId : '16164',
            type: type
        };
    
    ajaxSubmit(action,postData,validationcomplete,"POST","json",false);
}

function loadValidCSVTempData()
{
    window.location.href= 'https://staygrid.com/hmsadmine/importrsv/showcsvdata/hotelId/16164/cd/v';
}

function loadNonValidCSVTempData()
{
    window.location.href= 'https://staygrid.com/hmsadmine/importrsv/showcsvdata/hotelId/16164/cd/i';
}

function loadSucessCSVTempData()
{
    window.location.href= 'https://staygrid.com/hmsadmine/importrsv/showcsvdata/hotelId/16164/cd/s';
}

function loadFailedCSVTempData()
{
    window.location.href= 'https://staygrid.com/hmsadmine/importrsv/showcsvdata/hotelId/16164/cd/f';
}

function  successupload(data)
{
    if(data.error == 0)
    {
        var action = 'https://staygrid.com/hmsadmine/importrsv/inserttemprecords/';
        
        var postData = {
            'hotelId' : data.hotelId,
            'timestamp': data.time
        };
        $("#loader").show();
        ajaxSubmit(action,postData,successinsertrecords,"POST","json",false,$("#loader"));
    }
    else
    {
        $("#submit").prop('disabled',false);
        var messages = "<ul>";
        if($.isPlainObject(data.errorMessage))
        {
            $.each(data.errorMessage, function (index, value){
                messages += "<li>" + value + "</li>";
            });
        }
        else
        {
           messages +=  "<li>" + data.errorMessage + "</li>";
        }
        
        messages += "</ul>";
        $("#error").html(messages);
    }
}


function createreservationAjaxCall(step,totalSteps,limit,total,type)
{
    
    var fromRecord = (step*limit) - limit;
    var toRecord = (step*limit) - 1;
    var allTotal = $("#totalRecords").val();
    var compRec = parseInt(allTotal - total);
    var percent = Math.floor((compRec/allTotal)*100);
    $("#progressText").html("<br><br><strong>Records Remaining:</strong> "+total);
    jqueryProgressBar.progressbar.ajaxStartLoading(true,totalSteps,step,percent);
    
    var action = 'https://staygrid.com/hmsadmine/importrsv/createreservations/hotelId/16164/step/'+step;
    var postData = {
            'hotelId' : '16164',
             type: type
        };
    ajaxSubmit(action,postData,successcreatereservations,"POST","json",false);
}

function syncorupdateallotmentAjaxCall(step,totalSteps,limit,total,processGds)
{
    var fromRecord = (step*limit) - limit;
    var toRecord = (step*limit) - 1;
    var allTotal = $("#totalRecords").val();
    var compRec = parseInt(allTotal - total);
    var percent = Math.floor((compRec/allTotal)*100);
    $("#imgGds_"+processGds.gdsId).attr("src", '/images/opp-arrow.png');
    $("#imgRmTypeId_"+processGds.gdsId+"_"+processGds.rmTypeId).attr("src", '/images/wait.gif');
    console.log("Previous Gds: "+processGds.gdsId+" Rm Type: "+processGds.rmTypeId);
    $("#progressText").html("<br><br><strong>Gds Name:</strong> "+processGds.gdsName+"<br><strong>Room Code:</strong> "+processGds.rmTypeCode);
    jqueryProgressBar.progressbar.ajaxStartLoading(true,totalSteps,step,percent);
    $(".ui-dialog").css({top:'30px',position:'fixed'});
    
    var action = 'https://staygrid.com/hmsadmine/importrsv/syncallotment/hotelId/16164/step/'+step;
    var postData = {
            'hotelId' : '16164',
            'fromDate': (($("#fromDate"))?$("#fromDate").val():''),
            'toDate': (($("#toDate"))?$("#toDate").val():'')
        };
    ajaxSubmit(action,postData,successsyncallotments,"POST","json",false);
}

function successsyncallotments(data)
{
    if(data.error == 1)
    {
        /*
        var messages = "<ul>";
        if($.isPlainObject(data.errorMessage))
        {
            $.each(data.errorMessage, function (index, value){
                messages += "<li>" + value + "</li>";
            });
        }
        else
        {
           messages +=  "<li>" + data.errorMessage + "</li>";
        }
        
        messages += "</ul>";
        var previousContent = $("#progressError").html();
        if(previousContent != '')
        {
            messages += previousContent;
        }
        $("#progressError").html(messages); */
    }
    
    if(data.previousGds)
    {
        var imgName = 'on.GIF';
        if(data.error == 1)
        {
            imgName = 'off.GIF';
        }
        var previousGds = data.previousGds;
        if(previousGds.gdsId != data.processGds.gdsId)
        {
            $("#imgGds_"+previousGds.gdsId).attr("src", '/images/'+imgName);
        }
        
        $("#imgRmTypeId_"+previousGds.gdsId+"_"+previousGds.rmTypeId).attr("src", '/images/'+imgName);
        console.log("Previous Gds: "+previousGds.gdsId+" Rm Type: "+previousGds.rmTypeId);
    }
    if(data.complete == 0)
    {
        syncorupdateallotmentAjaxCall(data.step, data.totalsteps, data.limit, data.total, data.processGds);
    }
    else
    {
        jqueryProgressBar.progressbar.ajaxStopLoading();
        window.location.href= 'https://staygrid.com/hmsadmine/importrsv/updateallotment/hotelId/16164';
    }
}

function syncForMailChimpAjaxCall(step,totalSteps,limit,total,type)
{
    var fromRecord = (step*limit) - limit;
    var toRecord = (step*limit) - 1;
    var allTotal = $("#totalRecords").val();
    var compRec = parseInt(allTotal - total);
    var percent = Math.floor((compRec/allTotal)*100);
    $("#progressText").html("<br><br><strong>Sync For MailChimp:</strong> "+fromRecord+" - "+toRecord+" of "+allTotal+"");
    jqueryProgressBar.progressbar.ajaxStartLoading(true,totalSteps,step,percent);
    
    var action = 'https://staygrid.com/hmsadmine/importrsv/syncformailchimp/hotelId/16164/step/'+step;
    var postData = {
            'hotelId' : '16164',
             type: type
        };
    ajaxSubmit(action,postData,successsyncformailchimp,"POST","json",false);
}

function successsyncformailchimp(data)
{
    if(data.error == 0)
    {
        if(data.complete == 0)
        {
            syncForMailChimpAjaxCall(data.step, data.totalsteps, data.limit, data.total, data.type)
        }
        else
        {
            $("#recordType").val('SUCESS');
            jqueryProgressBar.progressbar.ajaxStopLoading();
            window.location.href= 'https://staygrid.com/hmsadmine/importrsv/showcsvdata/hotelId/16164/cd/s';
        }
    }
    else
    {
        jqueryProgressBar.progressbar.ajaxStopLoading();
        var messages = "<ul>";
        if($.isPlainObject(data.errorMessage))
        {
            $.each(data.errorMessage, function (index, value){
                messages += "<li>" + value + "</li>";
            });
        }
        else
        {
           messages +=  "<li>" + data.errorMessage + "</li>";
        }
        
        messages += "</ul>";
        $("#error").html(messages);
    }
}

function successinsertrecords(data)
{
