$("#finishImportRsv, #abortImportRsv").on('click', function (event){
    var prms = '';
    var type = '/type/INVALID';
    if($(this).attr('id') == 'finishImportRsv')
    {
        prms = '/finish/1';
    }
    else if($(this).attr('id') == 'abortImportRsv')
    {
        prms = '/abort/1';        
    }
    
    if(confirm("Do you wish to clear all data?"))
    {
        var action = "https://staygrid.com/hmsadmine/importrsv/deletetempdata/hotelId/16164"+prms+type;
        window.location.href=action;
    }
    
});

