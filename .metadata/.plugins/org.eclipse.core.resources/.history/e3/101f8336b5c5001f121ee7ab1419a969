    /*
     * force logout users with showing timer
     */
    var sec = 59;   // set the seconds
    var min = 03;   // set the minutes

    function logOutusers(frm,btnDiv,msgContainerId)
    {

        var prevHtml = $('#'+btnDiv).html();
        var newHtml = "<img src='https://12937.hotelogix.net/images/icon-save.gif' width='44' height='35' border='0' /><br />"+
                                 "Logging out user(s)";
        $('#'+btnDiv).html(newHtml);                                            

        var handleSuccess = function(o)
        { 
            if(o.responseText !== undefined)
            { 
                try 
                { 
                    countDown(frm,btnDiv,msgContainerId,prevHtml);
                }
                catch (x) 
                { 
                    alert("JSON Parse failed!"); 
                    return; 
                }
            }
        }

        var handleFailure = function(o)
        { 
            if(o.responseText !== undefined)
            { 
                return "Operation Failed";
            } 
        }

        var callback = { 
                success:handleSuccess, 
                failure: handleFailure, 
                argument: { foo:"foo", bar:"bar" } 
        }; 	

        sUrl = 'https://12937.hotelogix.net/admine/ajax/startlogedoutuser';
        var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback); 
    }

    function countDown(frm,btnDiv,msgContainerId,prevHtml) 
    {
       sec--;
        if (sec == -01) 
        {
         sec = 59;
         min = min - 1; 
        }
        else 
        {
         min = min; 
        }

        if (sec<=9) { sec = "0" + sec; }

        time = (min<=9 ? "0" + min : min) + " min : " + sec + " sec ";

        document.getElementById(msgContainerId).innerHTML = "<strong>Please wait. User(s) will be logged out within "+time+"</strong>";

        SD=window.setTimeout(function() { countDown(frm,btnDiv,msgContainerId,prevHtml);}, 1000);

        if (min == '00' && sec == '00') 
        { 
            sec = "00"; 
            window.clearTimeout(SD); 

            var handleSuccess = function(o)
            { 
                if(o.responseText !== undefined)
                { 
                   try { 
                        var messages =o.responseText;
                        $('#'+btnDiv).html(prevHtml);
                        frm.isFrontDeskLogedIn.value = 0;
                        document.getElementById(msgContainerId).innerHTML = " - User(s) logged out successfully. You may now continue to perform action.<br/>";

                    } catch (x) { 
                        alert("JSON Parse failed!"); 
                        return; 
                    } 
                }
            }
            var handleFailure = function(o)
            { 
                if(o.responseText !== undefined)
                { 
                    return "Operation Failed";
                } 
            }
            var callback = { 
                    success:handleSuccess, 
                    failure: handleFailure, 
                    argument: { foo:"foo", bar:"bar" } 
            }; 	

            sUrl = 'https://12937.hotelogix.net/admine/ajax/logedoutuser';
            var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback);

        }
    }
    
