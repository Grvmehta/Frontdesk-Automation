<script language="javascript">



function showStatistics ()
{       
       oPanelLoadingDivAdmin.show(); 
       var auditDate = '2019-04-16';
           
       var postdata={audit_date: auditDate};
        
       var sUrl="https://12937.hotelogix.net/admine/index/statistics";
            
        $.ajax (
                {
                    type: 'POST',
                    url: sUrl,
                    data: postdata,                       
                    dataType: 'html',
                    success: function (data) {
                        oPanelLoadingDivAdmin.hide();
                        $('.panel').hide();
                        $('.panelparent').addClass('stats').html(data);
                     },
                    error: function(xhr, err){
                        oPanelLoadingDivAdmin.hide();
                        if(xhr.readyState != 4 && xhr.status != 200)
                        {
                            alert(lang_mf_150);
                        }
                    }
                });
    
}

</script></td>
</tr>
