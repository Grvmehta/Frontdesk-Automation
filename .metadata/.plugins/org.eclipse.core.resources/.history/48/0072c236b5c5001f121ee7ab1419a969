            open: function (event, ui) {
                $('.ui-dialog').css({'z-index':103,'top': '50%', 'left': '50%', 'margin-left':'-42px', 'position': 'fixed'});
                $('.ui-widget-overlay').css('z-index',102);
            }
        });
        $("#loder").dialog("close");
    });
    
function saveInProcess(inProcess)
{
    if(inProcess){
        $("#loder").dialog("open");
    }else{
      $("#loder").dialog("close");
    }        
}

