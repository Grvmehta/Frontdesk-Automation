        MM_openBrWindow(url, '', 'width=600,height=320,scrollbars=1')
    }

    function importRsv(id)
    {

        window.open("https://staygrid.com/hmsadmine/importrsv/uploadcsv/hotelId/" + id, 'winPrev');
    }

    function actDeactHotel(id) {
        var url = "https://staygrid.com/hmsadmine/hotelsv2/changestatus/hotelid/" + id;
        window.open(url, '', 'top=300,left=400,width=400,height=320,scrollbars=1');
        return false;
    }
    
    
    function exportFile()
    {
        var displayFrom = '';
        var searchHid = '';
        var lastUsageDays = '';
        var billingCurrency = '';
        var searchHName = '';
        var hotelMode = '';
        var hotelStatus = '';
        var userWise = '';
        var totalFromDate = '';
        var serviceIdStr = '';
        var exportType = document.getElementById("selExport").value;
        var reportTitle = "Hotels New Reports";
        
        if(document.getElementById("displayFrom"))
        {
            displayFrom = document.getElementById("displayFrom").value;
        }
        if(document.getElementById("searchHid"))
        {
            searchHid = document.getElementById("searchHid").value;  
        }
        if(document.getElementById("lastUsageDays"))
        {
            lastUsageDays = document.getElementById("lastUsageDays").value;
        }
        if(document.getElementById("billingCurrency"))
        {
            billingCurrency = document.getElementById("billingCurrency").value;
        }
        if(document.getElementById("searchHName"))
        {
            searchHName = document.getElementById("searchHName").value;
        }
        if(document.getElementById("hotelMode"))
        {
            hotelMode = document.getElementById("hotelMode").value;
        }
        if(document.getElementById("hotelStatus"))
        {
            hotelStatus = document.getElementById("hotelStatus").value;
        }
        if(document.getElementById("userWise"))
        {
            userWise = document.getElementById("userWise").value;
        }
        if(document.getElementById("totalFromDate"))
        {
            totalFromDate = document.getElementById("totalFromDate").value;
        }
        if(document.getElementById("serviceIdStr"))
        {
            serviceIdStr = document.getElementById("serviceIdStr").value;
        }
        
        var postData = "?displayFrom="+ displayFrom + "&searchHid="+ searchHid + "&exportType="+ exportType +"&lastUsageDays="+ lastUsageDays + "&billingCurrency="+ billingCurrency + "&searchHName="+ searchHName + "&hotelMode="+ hotelMode + "&hotelStatus="+ hotelStatus + "&userWise="+ userWise + "&totalFromDate="+ totalFromDate + "&serviceIdStr="+ serviceIdStr;
        
        window.open("https://staygrid.com/hmsadmine/hotelsv2/hotelsexport/" + postData,"expPop","scrollbars=no,width=5,height=5,resizable=0,top=0,left=0");
       
    }
    
