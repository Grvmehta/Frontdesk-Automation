<script language="javascript">
    window.onload = initValues;
    function initValues()
    {
        var hotelCodeId = document.getElementById("hotelCodeId");
        var userName = document.getElementById("userNameId");
        var password = document.getElementById("passwordId");
        if (hotelCodeId.value == '')
        {
            hotelCodeId.focus();
            return false;
        }
        if (userName.value == '')
        {
            userName.focus();
        } else
        {
            password.focus();
        }
    }

    function submitLoginFrm()
    {
        var error = 0;
        var hId = '';
        var usnmId = '';
        var pwdId = '';
        var captchaId = '';
        
        var hotelCodeObj= document.frmCompanyInfo.hotelCode;
        var userNameObj = document.frmCompanyInfo.userNameId;
        var passwordObj = document.frmCompanyInfo.passwordId;
        var captchaObj  = document.frmCompanyInfo.txtCaptcha;
        
        hId = hotelCodeObj.value.trim();
        usnmId = userNameObj.value.trim();
        pwdId = passwordObj.value.trim();
        captchaId = captchaObj.value.trim();
        
        hotelCodeObj.className = "form-control";
        userNameObj.className = "form-control";
        passwordObj.className = "form-control";
        captchaObj.className = "form-control text-captcha";
        
        hotelCodeObj.parentElement.getElementsByTagName('p')[0].innerHTML='';
        userNameObj.parentElement.getElementsByTagName('p')[0].innerHTML='';
        passwordObj.parentElement.getElementsByTagName('p')[0].innerHTML='';
        captchaObj.parentElement.getElementsByTagName('p')[0].innerHTML='';
        
        
        var host = 'https://staygrid.com';
        reg = /^\s*\d+\s*$/;
        
        if (!reg.test(hId))
        {
            hotelCodeObj.className = "input-error form-control";
            hotelCodeObj.parentElement.getElementsByTagName('p')[0].innerHTML='Please provide valid hotel code.';
            error=1;
        }


        if (usnmId == '')
        {
            userNameObj.className = "input-error form-control";
            userNameObj.parentElement.getElementsByTagName('p')[0].innerHTML='Please provide user name.';
            error=1;
        }
        
        if(validateEmail(usnmId)==false)
        {
             userNameObj.className = "input-error form-control";
             userNameObj.parentElement.getElementsByTagName('p')[0].innerHTML='Please Enter Valid Email Address.';
             error=1;
        }

        if (pwdId == '')
        {
            passwordObj.className = "input-error form-control";
            passwordObj.parentElement.getElementsByTagName('p')[0].innerHTML='Please provide password.';
            error=1;
        }
        
        if (captchaId == '')
        {
            captchaObj.className = "input-error form-control text-captcha";
            captchaObj.parentElement.getElementsByTagName('p')[0].innerHTML='Please type the text for Image Verification.';
            error=1;
        }
        
        if (error != 0)
        {
            return false;

        }

        if (host.indexOf("upgraded.staygrid.com") == -1)
        {

            var hostArr = host.split("://");
            if (hId != '')
            {
                host = hostArr[0] + "://" + hId + "." + hostArr[1].replace('www.', '');
            }

        }

        var actionUrl = host + "/frontdesk/login/authenticate";
        var hrId = '';
        var campaignCode = '';
        if (hrId != '')
        {
            actionUrl += '?hr_id=' + hrId + '&amp;campaign_code=' + campaignCode;
        }

        document.frmCompanyInfo.action = actionUrl;

    }

    function requestForExtTrial(hotelCode)
    {
        var host = 'https://staygrid.com';
        document.frmCompanyInfo.action = host + "/frontdesk/login/requesttoextendtrial/hotelCode/" + hotelCode;
        document.frmCompanyInfo.submit();
    }

    function closeMe(closeId)
    {
        document.getElementById(closeId).style.display = 'none';
    }
    
    function validateEmail(email)
    { 

    var re = /^(([^&lt;&gt;()[\]\\.,;:\s@\"]+(\.[^&lt;&gt;()[\]\\.,;:\s@\"]+)*)|(\".+\"))@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\])|(([a-zA-Z\-0-9]+\.)+[a-zA-Z]{2,}))$/;
    return re.test(email);

    }
    
</script>
