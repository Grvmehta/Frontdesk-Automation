    <script language="javascript">
function doBack(frm) {
    
    var ac = "https://12738.hotelogix.net/V2/admine/packagedetails/listview";
    if(arguments[1] && arguments[1]!='')
    {
        ac = arguments[1];
    }
	frm.action = ac;
	frm.submit();
	
}
function fillData(act,vl,targetContainer,selectedValue) {

    var addSelOp = 0;

    if(arguments[4])
    {
        addSelOp = arguments[4];
    }
    
	var handleSuccess = function(o){ 
		if(o.responseText !== undefined){ 
			var messages = []; 
			
			var listObj = document.getElementById(targetContainer);
			
			listObj.length = 1;
			
	         try { 
     	       messages = YAHOO.lang.JSON.parse(o.responseText);

               if(messages==null)
               {
                return false;
               }
        	} catch (x) { 
	            alert("JSON Parse failed!"); 
	            return; 
	        }
	        
	        //Append Default
	       		var refChild = YAHOO.util.Dom.getChildren(listObj);
	        for (var i = 0, len = messages.length; i < len; ++i) { 
		         var m = messages[i];
		         var refChild = document.createElement('OPTION');
		         var refChild = new Option(m.field);
		         
 		         refChild.value = m.id;
 		         refChild.text = m.field;
 		         
 		         if(parseInt(selectedValue) == parseInt(m.id)) {
	 		         refChild.selected = true;
 		         }
                 if(addSelOp==1)
                 {
                    listObj[i] = refChild;
                 }else
                 {
                    listObj[i+1] = refChild;
                 }
	        }
		} 
	}
	

	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	

	sUrl = 'https://12738.hotelogix.net/V2/admine/ajax/'+act+'/id/'+vl;
	var request = YAHOO.util.Connect.asyncRequest('GET', sUrl, callback); 
	
}

function fillPosProAppTax(act,val,targetContainer,cnt, isInclusive)
{
	var listObj = document.getElementById(targetContainer);
	listObj.innerHTML='loading..';
	document.getElementById('btnSave').disabled=true; 
	var handleSuccess = function(o){ 
		if(o.responseText !== undefined){ 
			
			
	         try { 
     	       messages =o.responseText; 
			   listObj.innerHTML='';
			   listObj.innerHTML=messages;
			   document.getElementById('btnSave').disabled=false; 
        	} catch (x) { 
				 document.getElementById('btnSave').disabled=false; 
	            alert("JSON Parse failed!"); 
	            return; 
	        }
	        
	       
		} 
	}
	

	var handleFailure = function(o){ 
		 document.getElementById('btnSave').disabled=false; 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	

	sUrl = 'https://12738.hotelogix.net/V2/admine/ajax/'+act+'/posPointId/'+val+'/cnt/'+cnt+'/isinc/'+isInclusive;
	var request = YAHOO.util.Connect.asyncRequest('GET', sUrl, callback); 
}

function MM_goToURL() { //v3.0
  var i, args=MM_goToURL.arguments; document.MM_returnValue = false;
  for (i=0; i<(args.length-1); i+=2) eval(args[i]+".location='"+args[i+1]+"'");
}

function MM_openBrWindow(url,name,param) {
	window.open(url,name,param);
	return false;
}

function MM_openSurWindow(url,name,param) {
	window.open(url,name,param);
	return false;
}

function deleteImage(tableName,id,container,type,refId) {
	
	
	YAHOO.util.Dom.setStyle(container,'background','#ff0000');
	                
	if(!confirm('Do you really want to delete selected image.')) {
		YAHOO.util.Dom.setStyle(container,'background','#ffffff');
		return false;
	}
	
	
	var handleSuccess = function(o){ 
		if(o.responseText == 1) {
			YAHOO.util.Dom.setStyle(container,'display','none');
		}
		if(o.responseText == 0) {
			alert("Image could not Delete!");
		}
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	

	sUrl = 'https://12738.hotelogix.net/V2/admine/image/imagedelete/tablename/'+tableName+'/id/'+id+'/type/'+type+'/refid/'+refId;
	var request = YAHOO.util.Connect.asyncRequest('GET', sUrl, callback); 
	return false;

}

function deleteWebSettingPdf(tableName,colName,fieldUpdate,id,container) {
	
	
	YAHOO.util.Dom.setStyle(container,'background','#ff0000');
	
	if(!confirm('Do you Realy Want to delete select Pdf')) {
		YAHOO.util.Dom.setStyle(container,'background','#ffffff');
		return false;
	}
	
	
	var handleSuccess = function(o){ 
		if(o.responseText == 1) {
			YAHOO.util.Dom.setStyle(container,'display','none');
			YAHOO.util.Dom.get(fieldUpdate).value='';
			
		}
		if(o.responseText == 0) {
			alert("Image could not Delete!");
		}
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	

	sUrl = 'https://12738.hotelogix.net/V2/admine/image/webpdfdelete/tablename/'+tableName+'/colName/'+colName+'/id/'+id;
	var request = YAHOO.util.Connect.asyncRequest('GET', sUrl, callback); 
	return false;

}


function toggleTextImageCaption(box,label,act,txt,position) {
	var selectOpt = document.getElementById(box);
	var labelOpt = document.getElementById(label);
	
	
	if(act == 'edit') {
		
		YAHOO.util.Dom.setStyle(selectOpt,'display','');
		
		selectOpt.focus();
			
		YAHOO.util.Dom.setStyle(labelOpt,'display','none');
	}
	if(act == 'update') {
		YAHOO.util.Dom.setStyle(selectOpt,'display','none');
		if(position=='right')
			labelOpt.innerHTML = ''+selectOpt.value+' '+txt+'';
		else
			labelOpt.innerHTML = ''+txt+' '+selectOpt.value+'';
				
		YAHOO.util.Dom.setStyle(labelOpt,'display','');
	}
	return false;
	
}



	function removeLogoImage(container, logoHiddenField){
		YAHOO.util.Dom.setStyle(container,'background','#ff0000');
		
		if(!confirm('Do you Realy Want to delete the logo.') ) {
			YAHOO.util.Dom.setStyle(container,'background','#ffffff');
			return false;
		}
		YAHOO.util.Dom.setStyle(container,'display','none');
		YAHOO.util.Dom.get(logoHiddenField).value="";
	}

	var weekday=new Array(7);
	weekday[0]='Sunday';
	weekday[1]='Monday';
	weekday[2]='Tuesday';
	weekday[3]='Wednesday';
	weekday[4]='Thursday';
	weekday[5]='Friday';
	weekday[6]='Saturday';
	
	
	var monthsName = [];
	monthsName[1] = "January";
	monthsName[2] = "February";
	monthsName[3] = "March";
	monthsName[4] = "April";
	monthsName[5] = "May";
	monthsName[6] = "June";
	monthsName[7] = "July";
	monthsName[8] = "August";
	monthsName[9] = "September";
	monthsName[10] = "October";
	monthsName[11] = "November";
	monthsName[12] = "December";
	
	var monthsNameShort = [];
	monthsNameShort[1] = "Jan";
	monthsNameShort[2] = "Feb";
	monthsNameShort[3] = "Mar";
	monthsNameShort[4] = "Apr";
	monthsNameShort[5] = "May";
	monthsNameShort[6] = "Jun";
	monthsNameShort[7] = "Jul";
	monthsNameShort[8] = "Aug";
	monthsNameShort[9] = "Sep";
	monthsNameShort[10] = "Oct";
	monthsNameShort[11] = "Nov";
	monthsNameShort[12] = "Dec";	
	
	
	
	function convertDateFromMysqlToLocaleFormat(day,month,year) {
	
		var LOCALE_DATE_FORMAT="short";
 		var dat=day+'/'+month+'/'+year;
 		var d = new Date();
	 		d.setDate(day);
	 		d.setMonth(month-1);
			d.setYear(year);

		var dayOfDate=d.getDay();
 		var dayNameOfDate=weekday[dayOfDate];
 		
 		var monthName=monthsName[month];
 		
		var retDate = "";
		
 		if (LOCALE_DATE_FORMAT=='full') //  Wednesday, November 26, 2008
 			retDate = dayNameOfDate + ', ' + monthName + ' ' + day + ', ' + year;
 		if (LOCALE_DATE_FORMAT=='long')  //  November 26, 2008
			retDate = monthName + ' ' + day + ', ' + year;
 		if (LOCALE_DATE_FORMAT=='medium')  //  Nov 26, 2008
			retDate = monthsNameShort[month] + ' ' + day + ', ' + year;
 		if (LOCALE_DATE_FORMAT=='short')  //  01/26/08
			retDate = month + '/' + day + '/' + year;
 			
			
 		return retDate;
		
			
	}
	
	
	
	function changProductInInclution(posIdObj) {
	
		var posId=posIdObj.value;
		document.getElementById('prodIdMannual').disabled=true;
		var handleSuccess = function(o){
			if(o.responseText !== undefined){
				document.getElementById('posProListId').innerHTML=o.responseText;
				document.getElementById('prodIdMannual').disabled=false;
				
			}
		}
		
		var handleFailure = function(o){
			document.getElementById('prodIdMannual').disabled=false;
			if(o.responseText !== undefined){
				return "Operation Failed";
			}
		}
		var callback={
			success:handleSuccess,
			failure: handleFailure,
			argument: { foo:"foo", bar:"bar" }
		};
		
		sUrl = 'https://12738.hotelogix.net/V2/admine/packagedetails/changproduct/?posPointId='+posId;
		
		var request = YAHOO.util.Connect.asyncRequest('GET', sUrl, callback);
	}

	
	
	
