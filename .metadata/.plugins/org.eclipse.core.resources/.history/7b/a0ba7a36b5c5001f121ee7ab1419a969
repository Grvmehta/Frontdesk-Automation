<script type="text/javascript">
    function addItems(fromFieldName, toFieldName) 
    {
        var mainCurrListObj=YAHOO.util.Dom.get(fromFieldName);
        var mainSelectedList=YAHOO.util.Dom.getChildren(fromFieldName);
        var childCurrListObj=YAHOO.util.Dom.get(toFieldName);
        var childCurrListObjList=YAHOO.util.Dom.getChildren(toFieldName);

        for (var i = 0, len = mainSelectedList.length; i < len; ++i) 
        { 
            if(mainSelectedList[i].selected==true) 
            {
                var nText=mainSelectedList[i].text;
                var nkey=mainSelectedList[i].value;
                for (var z = 0, len1 = childCurrListObjList.length; z < len1; ++z) 
                { 
                        if (nkey==childCurrListObjList[z].value) 
                        { 
                                alert("This Item is already selected");
                                return false;
                        }
                }
                var refChild = mainSelectedList[i];
                childCurrListObj.appendChild(refChild);
            }		
        }
    }

    function removeItems(fromFieldName, toFieldName) 
    {
        var childCurrListObj=YAHOO.util.Dom.get(toFieldName);
        var childSelectedList=YAHOO.util.Dom.getChildren(toFieldName);
        var mainCurrListObj=YAHOO.util.Dom.get(fromFieldName);

        for (var i = 0, len = childSelectedList.length; i < len; ++i) 
        { 
            if(childSelectedList[i].selected) 
            {
                var nText=childSelectedList[i].text;
                var nkey=childSelectedList[i].value;
                var refChild = childSelectedList[i];
                mainCurrListObj.appendChild(refChild);
            }
        }
    }
    
