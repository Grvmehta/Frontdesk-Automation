    <link rel="stylesheet" type="text/css" href="https://12738.hotelogix.net/V2/jq/css/smoothness/jquery-ui-1.11.1.min.css">
    <link rel="stylesheet" type="text/css" href="https://12738.hotelogix.net/V2/jq/css/plugins/timepicker/timepicker.css">

    <script type="text/javascript" src="https://12738.hotelogix.net/V2/jq/js/jquery-ui-1.11.1.min.js"></script>
    <script type="text/javascript" src="https://12738.hotelogix.net/V2/jq/js/plugins/timepicker/jquery-ui-timepicker-addon.js"></script>
    
    <script type="text/javascript" src="https://12738.hotelogix.net/V2/js/ckeditor/ckeditor.js"></script>
    <script type="text/javascript" src="https://12738.hotelogix.net/V2/jq/js/jquery.dialogextend.js"></script>
   
        <!-- Bootstrap core JS -->
    <script language="javascript">
    
            var JQEditorsDom =[];
            
            
            var configSett = [
            ['Source', '-', 'NewPage','-','Undo','Redo','Preview'],
            ['Find','Replace','-','SelectAll','RemoveFormat'],
            ['Link', 'Unlink', 'Image','FontSize'],
            '/',
            ['Bold', 'Italic','Underline'],
            ['NumberedList','BulletedList','-','JustifyLeft','JustifyCenter','JustifyRight','Blockquote'],
            ['TextColor','BGColor', '-','SpecialChar', '-', 'Maximize']
            ];
            
            function setJQEditor(JQEditorHelperIds,type)
            {
                
                if(JQEditorHelperIds.length>0)
                {
                    if(type=='DEFAULT')
                    {
                        for(var i=0; i< JQEditorHelperIds.length; i++)
                        {
                            var editorId ='#'+JQEditorHelperIds[i];

                            if( $(editorId).length>0)
                            {
                                CKEDITOR.replace( JQEditorHelperIds[i] );
                            }
                        }
                    }
                    else if(type=='SHORT')
                    {
                        for(var i=0; i< JQEditorHelperIds.length; i++)
                        {
                            var editorId ='#'+JQEditorHelperIds[i];

                            if( $(editorId).length>0)
                            {
                                CKEDITOR.replace( JQEditorHelperIds[i],
                                 {toolbar :configSett}
                                );
                            }
                        }
                    }
                    else if(type=='SIMPLE')
                    {
                        
                        for(var i=0; i< JQEditorHelperIds.length; i++)
                        {
                            var editorId ='#'+JQEditorHelperIds[i];

                            if( $(editorId).length>0)
                            {
                               CKEDITOR.replace( JQEditorHelperIds[i],
                                {
                                    //extraPlugins : 'bbcode',
                                    toolbar :configSett
                                });
                                CKEDITOR.config.enterMode = CKEDITOR.ENTER_BR;   
                                CKEDITOR.config.protectedSource.push( /\[\[[\s\S]*?\]\]/g );   // [[tag]] code
                               // CKEDITOR.config.protectedSource.push( /\{\{[\s\S]*?\}\}/g );   // {{tag}} code        


                                CKEDITOR.on( 'instanceReady', function( ev )
                                { 
                                    var writer = ev.editor.dataProcessor.writer;             
                                    writer.indentationChars = ' ';
                                    var dtd = CKEDITOR.dtd;            
                                    for ( var e in CKEDITOR.tools.extend( {}, dtd.$block, dtd.$listItem, dtd.$tableContent ) )
                                    {
                                        ev.editor.dataProcessor.writer.setRules( e,
                                        {
                                        indent : true,
                                        breakBeforeOpen : true,
                                        breakAfterOpen : true,
                                        breakBeforeClose : true,
                                        breakAfterClose : true
                                        });
                                    }

                                });
                                
                                
                            }
                        }
                    }
                }    
            }
                

</script>
