<script>   
    jQuery('#listMenuRoot li').hover(function(){
        jQuery(this).children('.billing-submenu').show();
        jQuery(this).addClass('active');
    }).mouseleave(function(){
        jQuery(this).children('.billing-submenu').hide();
        jQuery(this).removeClass('active');
    });
function MM_openBrWindow(theURL,winName,features) 
{ //v2.0
  mychild = window.open(theURL,winName,features);
  mychild.focus();
}
function showAT()
{
    var hId = 0;
    var gdsType = '';
    var cntNm ='';
    var actNm ='';
    var hPmntGatewayId=0;
    if(document.getElementById("hotelId"))
    {
        hId = document.getElementById("hotelId").value;
    }
    if(document.getElementById("gdsType"))
    {
        gdsType = document.getElementById("gdsType").value;
    }
    if(document.getElementById("cntName"))
    {
        cntNm =  document.getElementById("cntName").value;
    }
    if(document.getElementById("actName"))
    {
        actNm =  document.getElementById("actName").value;
    }
    if(document.getElementById("hotelgatewayId"))
    {
        hPmntGatewayId =  document.getElementById("hotelgatewayId").value;
    }
    
   if(gdsType != '')
   {
       var url = "https://staygrid.com/hmsadmine/audittrail/viewat/cntName/"+cntNm+"/actName/"+actNm+"/hId/"+hId+"/gdsType/"+gdsType;
   }
   else if(hPmntGatewayId >0)
   {
       var url = "https://staygrid.com/hmsadmine/audittrail/viewat/cntName/"+cntNm+"/actName/"+actNm+"/hId/"+hId+"/hpmntGid/"+hPmntGatewayId;
   }
   else
   {    
       var url = "https://staygrid.com/hmsadmine/audittrail/viewat/cntName/"+cntNm+"/actName/"+actNm+"/hId/"+hId;

   }
    
    MM_openBrWindow(url, '', 'width=600,height=320,scrollbars=1')
}
  
</script>

