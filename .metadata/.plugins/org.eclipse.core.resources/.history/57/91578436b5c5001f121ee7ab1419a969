                        {
                        imgDiv.src =imgUrl + o.responseText;
			}
		}
		var handleFailure = function(o){
			if(o.responseText !== undefined){
				return "Operation Failed";
			}
		}
		var callback={
			success:handleSuccess,
			failure: handleFailure,
			argument: { foo:"foo", bar:"bar" }
		};
		
		sUrl = 'https://12937.hotelogix.net/admine/packagedetails/changerod/id/'+id;
		
		var request = YAHOO.util.Connect.asyncRequest('GET', sUrl, callback);
	}
    function changeFeatured(id, reftype, refId) {
        var imgUrl =  "https://12937.hotelogix.net/images/";
        var imgDiv = document.getElementById("feature_"+id);
        imgDiv.src = waitLoader.src;
        var handleSuccess = function(o){
            if(o.responseText !== undefined)
            {
                imgDiv.src =imgUrl + o.responseText;
                if(o.responseText != 'Star-empty.png')
                {
                    $("img[id^= feature_]").not("#feature_"+id).attr('src', imgUrl+ 'Star-empty.png');
                }
            }
        }
        var handleFailure = function(o){
                if(o.responseText !== undefined){
                        return "Operation Failed";
                }
        }
        var callback={
                success:handleSuccess,
                failure: handleFailure
        };

        sUrl = 'https://12937.hotelogix.net/admine/packagedetails/changefeature/id/'+id+ '/reftype/'+reftype+"/refId/"+refId;

       YAHOO.util.Connect.asyncRequest('GET', sUrl, callback);
    }
</script>

