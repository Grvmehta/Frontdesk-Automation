function selSrchOp(type,f='')
{    
    var posCatIds = [];
    
    $("#rpSrchPOSCATS").children("li").show();//show all parent cates

    $.each($("input[name='id[]']:checked"), function(){     
        var catId = $(this).val();
        $('#parentCatId'+catId).parent().parent().hide();
        posCatIds.push(catId);
    });

    if(posCatIds.length == 0)
    {
        alert("Please select category."); 
        return false;
    }
    
    $("#"+type+"tbl").toggle(); 
    var checkVisibleCats = $("#rpSrchPOSCATS").children("li:visible").length;
    if(f == '' && checkVisibleCats == 0)
    {
        $("#"+type+"tbl").hide(); 
        alert("Parent category not available to add."); 
        return false;
    }
    
    if(f == 1){
     type = type.split("-",1);
        addChildCatsToParent(type);
    }
    if(f == 2){
     clearAllOps(type);
    }
    
} 

function clearAllOps(type)
{
    var inputs, a, i;
    inputs = document.getElementsByClassName("selOps"+type);
    for (i = 0; i < inputs.length; i++) {         
        
        inputs[i].checked = false;
    }   
    document.getElementById("rpSrchClsPOSCATS").value = '';
}

function addChildCatsToParent(type)
{
    var selectedParentCats = $("input[name='parentCatId[]']:checked").length;
    if(selectedParentCats > 1)
    {
        alert("Please select only one parent category."); 
        $("#"+type+"tbl").show(); 
        return false;
    }
    if(selectedParentCats == 0)
    {
        alert("Please select a parent category."); 
        $("#"+type+"tbl").show(); 
        return false;
    }
     var posPointId = $("#posPointId").val();
     document.frmListView.action = 'https://12937.hotelogix.net/admine/packagedetails/addtoparent/'+posPointId;
     document.frmListView.submit();
}

function srchRpFun(type)
{
    var input, filter, ul, li, a, i;
    input = document.getElementById("rpSrchCls"+type);     
    filter = input.value.toUpperCase();
    
    ul = document.getElementById("rpSrch"+type);
    li = ul.getElementsByTagName("li");
    for (i = 0; i < li.length; i++) {         
        a = li[i].getElementsByTagName("span")[0]; 
        if (a.innerHTML.toUpperCase().indexOf(filter) > -1) {
            li[i].style.display = "";
        } else {
            li[i].style.display = "none";
        }
    }    
    
    $.each($("input[name='id[]']:checked"), function(){     
        var catId = $(this).val();
        $('#parentCatId'+catId).parent().parent().hide();
    });
} 

function selAllOps(type)
{
    var inputs, a, i;
    inputs = document.getElementsByClassName("selOps"+type);
    for (i = 0; i < inputs.length; i++) {         
        
        inputs[i].checked = true;
    }   
}
</script>

	


