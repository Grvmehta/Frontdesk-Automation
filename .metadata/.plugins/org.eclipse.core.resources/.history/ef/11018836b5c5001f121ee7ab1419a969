        function clickToGTranslateV2(srcGBox,destGBox,translateFromLang,translateToLang,yuiEditor)
        {  
                var srctxt=$('#'+srcGBox).val();
                if(srctxt.length>1000)
                {
                    alert("Limit for Autotext translate is 1000 characters only.\n Please do a manual translation or reduce the text size.");
                }
                else
                {
                  
                  var postData = {fromCode: translateFromLang,toCode: translateToLang,srctxt: srctxt};
                  
                  var actionPost = 'https://qa1hz.stayez.com/admine/ajax/dotranslate/';
                  
                    $.ajax({
                        url: actionPost,
                        type: "POST",
                        dataType: "json",
                        data: postData,
                        crossDomain: false,            
                        error: function (jqXHR, exception) {

                        },
                        success: function( response ) {
                            
                                   if (response.data)
                                   {
                                       $('#'+destGBox).val(response.data.translations);
                                   }
                                   else if(response.error)
                                   {
                                        $('#'+destGBox).val('');
                                        alert(response.error.message);
                                   }    
                         
                        }
                    });
                  
                }
            

        }

	function html_entity_decode_gtranslate( string ) { 
		var ret, tarea = document.createElement('textarea');
		tarea.innerHTML = string;
		ret = tarea.value;
		return ret;
	}
function jqueryDialog(dialogElemObj, dialogExtendParams, isModal)
    {
        if(typeof dialogExtendParams == 'undefined')
        {
            var dialogExtendOptions = {
                "closable" : true,
                "maximizable" : true,
                "minimizable" : true,
                "minimizeLocation" : 'right',
                "collapsable" : true,
                "dblclick" : 'maximize'
              };
          }
          else
          {
             var dialogExtendOptions = dialogExtendParams;
          }

        $(dialogElemObj).dialog({modal: ((typeof isModal != 'undefined')?isModal:true),
                closeOnEscape: false,
                minHeight:400,
                minWidth:850,
                resizable:false,
                autoOpen: false
        }).dialogExtend(dialogExtendOptions);
    }
    
    var bookingSrcdialogExtendParams = {
        "closable" : true,
        "maximizable" : true,
        "minimizable" : true,
        "minimizeLocation" : 'right',
        "collapsable" : true,
        "dblclick" : 'maximize'
      };
      
      jqueryDialog($("#bookingSuiteRateIframe"), bookingSrcdialogExtendParams, false);
      
      $("#bookingSuiteIframeOpen, #bookingSuiteIframeOpen1").on("click", function (){
        var actionUrl = 'https://qa1hz.stayez.com/admine/index/bookingsuiteiframe';
        $("#loadImg").show();
        $.get(actionUrl, function(response, status, xhr ) {
              var jsonParsedData = $.parseJSON(response);

              if (status == "error" || jsonParsedData.error == 1)
              {
                  var errorMessage = (status == "error")?xhr.statusText:((jsonParsedData.error == 1)?jsonParsedData.message:'');
                  $("#errorMessage").html(errorMessage);
                  $("#errorMessage").show();
                  $("#bookingSuiteRateIframe").dialog('open');
              }
              else
              {
                  $("#errorMessage").html("");
                  $("#errorMessage").hide();
                  var bsHeight = (window.innerHeight)-100;
                  console.log(bsHeight);
                  var iframeUrl = jsonParsedData.iframeUrl;
                  $("#bookingSuitePreview").attr('src', iframeUrl);
                  $("#bookingSuiteRateIframe").css({top:'0px'});
                  $("#bookingSuiteRateIframe").dialog('option','minWidth',950);
                  $("#bookingSuiteRateIframe").dialog('option','minHeight',bsHeight);
                  $("#bookingSuiteRateIframe").dialog('option','resizable',true);
                  $(".ui-dialog").css({top:'0px',position:'fixed'});
                  $("#bookingSuitePreview").attr('height',(bsHeight)-70);
                  $("#bookingSuitePreview").show();
                  $("#bookingSuiteRateIframe").dialog('open');
              }
        });
    });
    
    $('#bookingSuiteRateIframe').on('dialogclose', function(event) {
        $("#bookingSuitePreview").attr('src', "");
        $("#bookingSuitePreview").hide();
        $("#errorMessage").html("");
        $("#errorMessage").hide();
    });
    $("#bookingSuiteRateIframe").on("dialogresize", function( event, ui ) {
            var bsHeight = (window.innerHeight);
            console.log(bsHeight);
            $("#bookingSuiteRateIframe").dialog('option','minHeight',bsHeight);
            $("#bookingSuitePreview").attr('height',(bsHeight)-70);
    });
    $(document).ready(function() {
        $('iframe').load(function() {
            $("#loadImg").hide();
        });
    });
