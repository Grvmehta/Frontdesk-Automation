function changePrecisionAction(id, pricision) {
		var handleSuccess = function(o){ 
			if(o.responseText=='accessDenied'){
				window.location.href="https://hlxpms.net/admine/unauthorized/noper/";
			}else{
				return o.responseText;
			}
		}
		var handleFailure = function(o){ 
			if(o.responseText !== undefined){ 
				return "Operation Failed";
			} 
		}
		var callback = { 
			success:handleSuccess, 
			failure: handleFailure, 
			argument: { foo:"foo", bar:"bar" } 
		};
		sUrl = 'https://hlxpms.net/hmsadmine/hotelsv2/changePrecision/id/'+id+'/pricision/'+pricision+'/';
		var request = YAHOO.util.Connect.asyncRequest('GET', sUrl, callback); 
}



function deleteSelected_v2(totalCtrl) {
				
		
		var frm = document.frmListView;
		var chkBox = null;
		
		var idObject = new Array();
		var j = 0;
		for(i = 1; i <= totalCtrl; ++i) {
			chkBox = document.getElementById("id_"+i);
			if(chkBox.checked == true) {	
				j++;
			}
		}
		
		if(j > 0) {
			if(!confirm("Do You really want to Delete Selected Records")) {
				return 0;
			}
		}
		
		if (j ==0) return;
		j = 0;
		YAHOO.example.container.wait.show();

		for(i = 1; i <= totalCtrl; ++i) {
			chkBox = document.getElementById("id_"+i);
			
			if(chkBox.checked == true) {
				idObject[j] = chkBox.value;
				++j;
				chkBox.checked = false;
			}
		}
		
		
		var handleSuccess = function(o) { 
			var idArray = []; 
			if(o.responseText !== undefined){ 
				try { 
					idArray = YAHOO.lang.JSON.parse(o.responseText); 
				} 
				catch (x) { 
					alert("JSON Parse failed!"); 
						YAHOO.example.container.wait.hide();
					return; 
				}
				 for (var i = 0, len = idArray.length; i < len; ++i) { 
					 	YAHOO.util.Dom.setStyle("row_"+idArray[i], "display" , "none");
					 	var rowObj = YAHOO.util.Dom.get("row_"+ idArray[i]);
					 	colObj = YAHOO.util.Dom.getFirstChild("row_"+ idArray[i]);
					 	colObj.innerHTML = 0;
					 	
					 	
				 }
				 var sn = 1;
				 for(i  = 1; i <=totalCtrl; ++i) {
					 Obj = YAHOO.util.Dom.get("col_"+ i);
					 
					 var num = parseInt(Obj.innerHTML);
					 if(num != 0) {
						 Obj.innerHTML = sn;
						 ++sn;
					 }
					 
				 }
				 
				YAHOO.example.container.wait.hide();
			} 
		}
		
		var handleFailure = function(o){ 
			if(o.responseText !== undefined){ 
				YAHOO.example.container.wait.hide();
				return "Operation Failed";
			} 
		}
		var callback = { 
			success:handleSuccess, 
			failure: handleFailure, 
			argument: { foo:"foo", bar:"bar" } 
		}; 	
	
		sUrl = 'https://hlxpms.net/hmsadmine/hotelsv2/delete';
		postData = "ids="+idObject;
		var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback, postData); 
		
}


function MM_goToURL() { //v3.0
  var i, args=MM_goToURL.arguments; document.MM_returnValue = false;
  for (i=0; i<(args.length-1); i+=2) eval(args[i]+".location='"+args[i+1]+"'");
}

function MM_openBrWindow(theURL,winName,features) { //v2.0
  window.open(theURL,winName,features);
}

function openBrWindowInv(e,theURL,winName,features) { //v2.0

	var target = YAHOO.util.Event.getTarget(e);

	var tag = target.tagName;
	if(tag.toLowerCase()=='input'){
		return false;
	}else
	{
		MM_openBrWindow(theURL,winName,features);
	}
	
	
  
}

function calDates(sId, newVal, curDayVal, curMonthVal, eId, currId, firstId, lastId, setValToControl ) {
	
	var daysDom=' ';
	var monthsDom=' ';
	var daysInMonthListDom=' ';
	var daysDom=daysDom.split(",");
	monthsDom=monthsDom.split(",");
	daysInMonthListDom=daysInMonthListDom.split(",");
	
	var sDays=YAHOO.util.Dom.get(sId);
	var sMonths=YAHOO.util.Dom.get("startMonths_"+ parseInt(currId));
	
	//alert(" curr id ="+currId);
	
	//alert(" lastId id ="+lastId);

	var newVal=newVal;
	var newValMonth;
	//alert(newVal);
	if (parseInt(daysInMonthListDom[sMonths.value-1]) < parseInt( newVal)) {
		alert("Not a valid Date");
		sDays.value=curDayVal;
		return false;
	}
	if(parseInt(currId)==parseInt(firstId)) {
		var x=parseInt(lastId);
		var eDate=YAHOO.util.Dom.get("endDatDay_"+parseInt(x));
		var endDays=YAHOO.util.Dom.get("endDays_"+parseInt(x));
		var endMonths=YAHOO.util.Dom.get("endMonths_"+parseInt(x));
	}
	if(parseInt(currId)==parseInt(lastId)) {
		var x=parseInt(currId)-1;
		if (x<=0 ) x=1;
		var eDate=YAHOO.util.Dom.get("endDatDay_"+parseInt(x));
		var endDays=YAHOO.util.Dom.get("endDays_"+parseInt(x));
		var endMonths=YAHOO.util.Dom.get("endMonths_"+parseInt(x));
	}
	
	if(parseInt(currId)!=parseInt(lastId) && parseInt(currId)!=parseInt(firstId) ) {
		var x=parseInt(currId)-1;
		if (x<=0 ) x=1;
		//alert(x);
		var eDate=YAHOO.util.Dom.get("endDatDay_"+parseInt(x));
		var endDays=YAHOO.util.Dom.get("endDays_"+parseInt(x));
		var endMonths=YAHOO.util.Dom.get("endMonths_"+parseInt(x));
	}

	
	var nDateStr="";
	
	if(newVal==1) {
		if (sMonths.value==1) {
			newVal=daysInMonthListDom[monthsDom.length -1];
			nDateStr = newVal+ ' ' + monthsDom[monthsDom.length -1];
			//endDays.value=newVal;
			//endMonths.value=monthsDom.length;
			newValMonth=monthsDom.length;
		} else {
			newVal=daysInMonthListDom[sMonths.value-2];
			newVal=parseInt(daysInMonthListDom[sMonths.value-2]);
			nDateStr = newVal+ ' ' + monthsDom[parseInt(curMonthVal)-2];
			//endDays.value=newVal;
			//endMonths.value=parseInt(curMonthVal)-1;
			newValMonth=parseInt(curMonthVal)-1;
		}
	}else {
		newVal=parseInt(newVal)-1;
		nDateStr = newVal+ ' ' + monthsDom[sMonths.value-1];
		newValMonth=parseInt(sMonths.value);
	}
	
	if (setValToControl==1) {
		endDays.value=newVal;
		endMonths.value=newValMonth;
		eDate.innerHTML=nDateStr;
	} else {
		return endDays.value +", "+endMonths.value
	}
}




function customChargeAdd(controler)
{
	frm = document.frmListView;
	frm.hidCh.value=1;
	
	var chAmount = frm.chAmount.value;			

	if(chAmount == "") {
		alert("Please Enter Amount.");
		return false;
	}
	frm.action=controler+"/addcustomcharge/";
	frm.submit();
}
	
function toggleSelectPay(fl) {

			var i = 1;
		
	for(j=1; j <= i; j++) {
		if(document.getElementById("id_"+j).disabled==false)
			document.getElementById("id_"+j).checked = fl;
	}
	
	getPayAmount();
}
		
		
function getPayAmount()
{
	
	var amount = 0;
			var i = 1;
		var count = 0;
	
	for(j=1; j <= i; j++) {
		if(document.getElementById("id_"+j).disabled==false)
		{
			if(document.getElementById("id_"+j).checked)
			{
				
				amount+=parseFloat(document.getElementById("id_"+j).value);
				
				count++;
			}
		}	
	}	
	setPayAmount(amount)
	
	if(count>1)
		document.getElementById("amount").disabled=true;
	else
		document.getElementById("amount").disabled=false;
		
}	

function setPayAmount(amount)
{

	var amtBox =  document.getElementById("amount");
	
	amtBox.value = '';
	amtBox.value=roundNumber(amount,2);

}		

function payAmount(controler)
{
	frm = document.frmListView;
	frm.hidPay.value=1;
	
			var payTypesMode = frm.payTypesMode.value;			
			var payTypes = frm.payTypes.value;		
			var txtAmount = frm.amount.value;			
			var txtCcChequeNo = frm.ccChequeNo.value;
			var txtReceiptNo = frm.receiptNo.value;
			var txtDesc = frm.description.value;	
			
							var i = 1;
						
			var flag=false;
			for(j=1; j <= i; j++) {
				if(document.getElementById("id_"+j).checked)
				{
					flag=true;
				}
			}
			
			if(!flag)
			{
				alert("Please Select Invoice.");
				return false;
			}
	
			if(payTypesMode == "") {
				alert("Please Select Payment Mode");
				return false;
			}
			
			if(payTypes == "")
			{
				alert("Please Select Payment Type");
				return false;
			}
			
			if(txtAmount == "" || parseFloat(txtAmount) == 0) {
				alert("Please Enter Amount");
				return false;
			}
			
			if(payTypesMode == "cc")
			{			
				if(txtCcChequeNo == "")
				{
					alert("Please Enter Credit Card No.");
					return false;
				}			
			}
			
			if(payTypesMode == "cheque")
			{
				if(txtCcChequeNo == "")
				{
					alert("Please Enter Cheque No.");
					return false;
				}			
			}
			frm.action=controler+"/payamount/";
			frm.submit();
}

function roundNumber(num, dec)
{
	var result = Math.round( num * Math.pow( 10 , dec ) ) / Math.pow( 10 , dec );
	
	//var result=YAHOO.util.Number.format(num,{decimalPlaces :dec});
	
	return result;
}
function checkSelected(totalRec)
{
		var i =0;
		var j = 0;
		
	
		for(i = 1; i <= totalRec; ++i) {
			chkBox = document.getElementById("id_"+i);
			if(chkBox.checked == true) {	
				j++;
			}
		}
		
        if (j == 0) 
        {
             alert("Please select record(s).");
             return false;
        }
           
        
		if(j > 0)
        {
			if(!confirm("Do You really want to Delete Selected Records."))
            {
				return false;
			}else
            {
                  return true;  
            }
		}
		
}

/**
* redirect page with export params 
**/
function exportInvoices()
{
   document.frmListView.isExport.value = 1;
   document.frmListView.submit();
   document.frmListView.isExport.value = '';
}

</script>
	
<script language="javascript">


    function toggleSelectAllProp(fl){
            $(".propsClass").prop('checked',fl);
    }

    function toggleSelectProp(fl){
        if($(".propsClass").length == $(".propsClass:checked").length) {
            $(".propsAllClass").prop("checked", true);
        } else {
            $(".propsAllClass").prop("checked", false);
        }
		}

  
    function toggleSelect(fl) {
        var i = 10;
        for (j = 1; j <= i; j++) {

            if (document.getElementById("chstatus_" + j))
            {
                document.getElementById("chstatus_" + j).checked = fl;
            }
        }
    }

    function chStatus()
    {
        var i = 0;
        var j = 0;


        for (i = 1; i <= 10; ++i) {

            if (document.getElementById("chstatus_" + i))
            {
                if (document.getElementById("chstatus_" + i).checked == true) {
                    j++;
                }
            }
        }

        if (j == 0)
        {
            alert("Please select hotel(s) to deactivate.");
        } else
        {
            document.frmListView.submit();
        }
    }

    function handleSelect(type, args, obj) {
        var dates = args[0];
        var date = dates[0];
        var year = date[0], month = date[1], day = date[2];
        if (parseInt(day) < 10)
            day = '0' + day;
        if (parseInt(month) < 10)
            month = '0' + month;
        var txtDate1 = document.getElementById("displayFrom");
        txtDate1.value = year + "-" + month + "-" + day;
        obj.hide();
    }
    
    function formCheck(frm) {

        var billingCurrency = frm.billingCurrency.value;
        var totalBalance = frm.totalBalance.value;


        if (billingCurrency != '' || totalBalance != '')
        {
            if (billingCurrency == '' && totalBalance != '')
            {
                alert('Select Currency!');
                return false;
            }
            if (billingCurrency != '' && totalBalance == '')
            {
                alert('Enter Total Balance!');
                return false;
            }
        }
        return true;
    }

    function customSettings(id)
    {
        var url = "https://hlxpms.net/hmsadmine/customsettings/editview/hotelId/" + id;
        MM_openBrWindow(url, '', 'width=600,height=320,scrollbars=1')
    }

    function importRsv(id)
    {

        window.open("https://hlxpms.net/hmsadmine/importrsv/uploadcsv/hotelId/" + id, 'winPrev');
    }

    function actDeactHotel(id) {
        var url = "https://hlxpms.net/hmsadmine/hotelsv2/changestatus/hotelid/" + id;
        window.open(url, '', 'top=300,left=400,width=400,height=320,scrollbars=1');
        return false;
    }
    
    
    function exportFile()
    {
        var displayFrom = '';
        var searchHid = '';
        var lastUsageDays = '';
        var billingCurrency = '';
        var searchHName = '';
        var hotelMode = '';
        var hotelStatus = '';
        var userWise = '';
        var totalFromDate = '';
        var serviceIdStr = '';
        var exportType = document.getElementById("selExport").value;
        var reportTitle = "Hotels New Reports";
        
        if(document.getElementById("displayFrom"))
        {
            displayFrom = document.getElementById("displayFrom").value;
        }
        if(document.getElementById("searchHid"))
        {
            searchHid = document.getElementById("searchHid").value;  
        }
        if(document.getElementById("lastUsageDays"))
        {
            lastUsageDays = document.getElementById("lastUsageDays").value;
        }
        if(document.getElementById("billingCurrency"))
        {
            billingCurrency = document.getElementById("billingCurrency").value;
        }
        if(document.getElementById("searchHName"))
        {
            searchHName = document.getElementById("searchHName").value;
        }
        if(document.getElementById("hotelMode"))
        {
            hotelMode = document.getElementById("hotelMode").value;
        }
        if(document.getElementById("hotelStatus"))
        {
            hotelStatus = document.getElementById("hotelStatus").value;
        }
        if(document.getElementById("userWise"))
        {
            userWise = document.getElementById("userWise").value;
        }
        if(document.getElementById("totalFromDate"))
        {
            totalFromDate = document.getElementById("totalFromDate").value;
        }
        if(document.getElementById("serviceIdStr"))
        {
            serviceIdStr = document.getElementById("serviceIdStr").value;
        }
        
        var postData = "?displayFrom="+ displayFrom + "&searchHid="+ searchHid + "&exportType="+ exportType +"&lastUsageDays="+ lastUsageDays + "&billingCurrency="+ billingCurrency + "&searchHName="+ searchHName + "&hotelMode="+ hotelMode + "&hotelStatus="+ hotelStatus + "&userWise="+ userWise + "&totalFromDate="+ totalFromDate + "&serviceIdStr="+ serviceIdStr;
        
        window.open("https://hlxpms.net/hmsadmine/hotelsv2/hotelsexport/" + postData,"expPop","scrollbars=no,width=5,height=5,resizable=0,top=0,left=0");
       
    }
    
</script>
<script language="javascript">
    
    $(function() {    
        $( "#billingDetailsLoader" ).dialog({
            width: 40,         
            maxWidth: 40,
            height: 70,
            modal: true,
            create: function (event, ui) {
                $(".ui-dialog-titlebar").attr('style','display:none;');
                $(".ui-dialog-content").css({textAlign:'center'});
            }
        });

        $("#billingDetailsLoader").dialog("close");

    });    


    function showBillingDetails(hId)
    {        
        $("#billingDetailsLoader").dialog("open");
       
        var actionc = '/hmsadmine/hotelsv2/showbillingdetails';
        $.ajax({
            url: actionc,
            type: "POST",
            dataType: "html",
            data: {hotelId:hId},
            crossDomain: false,
            error: function (jqXHR, exception) {
                $("#billingDetailsLoader").dialog("close");
            },
            success: function( data ) {
                $("#billingDetailsLoader").dialog("close");
                if(data != '')
                {
                    $("#hotelBillingDetails").html(data);
                    $("#hotelBillingDetails").dialog({
                        modal: true,
                        closeOnEscape: true,
                        resizable:false,
                        width:'auto',
                        title:'Billing Details',
                    });
                }
            }
        });
    }
</script>

