<script language="javascript">
function Navigate(o) {
   
	if(document.frmListView.maxEntries && parseInt(document.frmListView.maxEntries.value)>=100000){
		if(!confirm('You have selected to view all records on a single page.\nIt may take some time to show all the records.\nIf the number of records are too much then it may misconfigure your server.\nDo you want to continue?')){
			return false;
		}
	}
	
	document.frmListView.page.value = o;
	document.frmListView.offset.value = o;
	document.frmListView.submit();
	
}

function submitForm() {
	document.frmListView.submit();
}

function submitForm4Rmtype() {
	document.frmListView.page.value = 0;
	document.frmListView.offset.value = 0;
	document.frmListView.submit();
	
}

function doSort(fld, order_by) {
	document.frmListView.orderBy.value = fld;
	document.frmListView.sortOrder.value = order_by;
	document.frmListView.submit();
}
function toggleSelect(fl) {
	var i = 1;
	for(j=1; j <= i; j++) {
		document.getElementById("id_"+j).checked = fl;
	}
}
function addNew(act) {
	toggleSelect(false);
	if(!act) {
		document.frmListView.action = "https://hlxpms.net/hmsadmine/hotelsv2/editview/attamp/1";
	}else {
		document.frmListView.action = "https://hlxpms.net/hmsadmine/hotelsv2/"+act;
	}
	document.frmListView.submit();
	
}

	function editMultiple(act,totalCtrl) {
		var i =0;
		var j = 0;
		for(i = 1; i <= totalCtrl; ++i) {
			chkBox = document.getElementById("id_"+i);
			if(chkBox.checked == true) {	
				j++;
			}
		}
		if (j == 0) {
                    return false;
                }
		
		document.frmListView.action = "https://hlxpms.net/hmsadmine/hotelsv2/"+act;
		document.frmListView.submit();
	}

	function deleteSelected(totalCtrl) {
		var i =0;
		var j = 0;
		
	
		for(i = 1; i <= totalCtrl; ++i) {
			chkBox = document.getElementById("id_"+i);
			if(chkBox.checked == true) {	
				j++;
			}
		}
		
		if(j > 0) {
			if(!confirm("Do You really want to Delete Selected Records")) {
				return 0;
			}
		}
		
		if (j ==0) return;
			document.frmListView.action = "https://hlxpms.net/hmsadmine/hotelsv2/delete";
			document.frmListView.submit();
	}
 function changeBAlertStatus(id) {
var msg;
msg=document.getElementById("billAlert_"+id).src;
var ms=msg.split('/');
var s;
for(var i=0;i<ms.length;i++){
s=ms[i];
}
if(s=='off.GIF'){
msg='Do You really want to Activate Billing Alert?';
}else{
msg='Do You really want to De-Activate Billing Alert?';
}

var i=confirm(msg);
if(!i){
     return false;
}

        var actionName = "";
        var container = "";
        if(arguments[1])
        {
            actionName  = arguments[1];
            
        }
        if(arguments[2])
        {
            container  = arguments[2];

        }

		var imgUrl =  "https://hlxpms.net/images/";
        if(container!='')
        {
            var imgDiv = document.getElementById(container+id);
        }else
        {
            var imgDiv = document.getElementById("billAlert_"+id);

        }
		imgDiv.src = waitLoader.src;
		var handleSuccess = function(o){
			if(o.responseText !== undefined){
				if(o.responseText=='accessDenied'){
					window.location.href="https://hlxpms.net/admine/unauthorized/noper/";
				}else{
					imgDiv.src =imgUrl + o.responseText;
					return o.responseText;
				}
			}
		}
		var handleFailure = function(o){
			if(o.responseText !== undefined){
				return "Operation Failed";
			}
		}
		var callback={
			success:handleSuccess,
			failure: handleFailure,
			argument: { foo:"foo", bar:"bar" }
		};

        if(actionName!='')
        {
            sUrl = 'https://hlxpms.net/hmsadmine/hotelsv2/'+actionName+'/id/'+id;
        }else
        {
            sUrl = 'https://hlxpms.net/hmsadmine/hotelsv2/changebalertstatus/id/'+id;
        }
		
		var request = YAHOO.util.Connect.asyncRequest('GET', sUrl, callback);
	}

	function changStatus(id) {

        
        
        var actionName = "";
        var container = "";
        if(arguments[1])
        {
