{
    window.addEventListener('online', function () { 
        console.log('Internet is connected.');
        $("#detectInternetConnOFF").hide();
    });
    window.addEventListener('offline', function () {
        console.log('Internet is disconnected.');
        $("#detectInternetConnOFF").show();
    });
});    
    
      $(function() {
        $( "#loder" ).dialog({
            width: 'auto',
            height: 70,
            modal: true,
            create: function (event, ui) {
                $(".ui-dialog-titlebar").attr('style','display:none;');
                $(".ui-dialog-content").css({textAlign:'center'});       
            },
            open: function (event, ui) {
                $('.ui-dialog').css({'z-index':103,'top': '50%', 'left': '50%', 'margin-left':'-42px', 'position': 'fixed'});
                $('.ui-widget-overlay').css('z-index',102);
            }
        });
        $("#loder").dialog("close");
    });
    
function saveInProcess(inProcess)
{
    if(inProcess){
        $("#loder").dialog("open");
    }else{
      $("#loder").dialog("close");
    }        
}

