<script language="javascript">
    // Calling function to Initialize the Operations.
    function toggleChk(chk) {

        var totalChk = document.getElementById("totalChk").value;
        for(i = 1; i <= totalChk; ++i) {
            document.getElementById("ids_"+i).checked = chk;
        }
    }
    
    function openproperty(Obj, currHotelId)
    {
        if(Obj.id != currHotelId)
        {
            var newPropertyId = Obj.id;
           
            var host = 'https://hotelogix.net';
            var localIpDomain = 'hz2.stayezee.com';
            host = hostnameWithHotelID(host, localIpDomain, newPropertyId);
            var openUrl = host+"/frontdesk/login/checksessionexit/prop/"+newPropertyId+"/chid/"+currHotelId;
            var win = window.open(openUrl, '_blank');
                win.focus();
            Obj.id = currHotelId;    
        }
    }
    
    function loadPicker()
    {
         $( "#nauditDateFailedQueue, #nauditDateFailedQueue1" ).datepicker({showOn: "button",
                                                   buttonImage: "https://12937.hotelogix.net/images/calendar.gif",
                                                   buttonImageOnly: true,
                                                   dateFormat : 'yy-mm-dd'
                                                 });    
         $( "#nauditDateFailedQueue, #nauditDateFailedQueue1" ).datepicker("setDate", new Date());                                             
    }

    
    function jqueryDialog(dialogElemObj, dialogExtendParams)
    {
        if(typeof dialogExtendParams == 'undefined')
        {
            var dialogExtendOptions = {
                "closable" : true,
                "maximizable" : true,
                "minimizable" : true,
                "minimizeLocation" : 'right',
                "collapsable" : true,
                "dblclick" : 'collapse'
              };
          }
          else
          {
             var dialogExtendOptions = dialogExtendParams;
          }

        $(dialogElemObj).dialog({modal: true,
                closeOnEscape: false,
                minHeight:400,
                minWidth:850,
                resizable:false,
                autoOpen: false
        }).dialogExtend(dialogExtendOptions);
    }
    
    var snpdialogExtendParams = {
        "closable" : true,
        "maximizable" : false,
        "minimizable" : true,
        "minimizeLocation" : 'left',
        "collapsable" : false,
        "dblclick" : ''
      };
    jqueryDialog($("#snapshotDashboard"), snpdialogExtendParams);
    
 
    
    $("#snapshotOptin").on("click", function () {
        var snpStatus = "";
        
        if(snpStatus == 'ACTIVE')
        {
            var hotelMode = "TRIAL";
            if(hotelMode != 'PAID')
            {
                alert("To activate FREE SnapShot Analytics, please upgrade your account to a Premium plan. For more details, contact support@hotelogix.com.");
                return false;
            }
            else
            {
                var Url = "https://www.youtube.com/embed/2eyVxx1kih0?autoplay=1";
                window.open(Url, "SnapshotDashboardLogin", "status=1,resizable=1,width=1180,height=750,scrollbars=1");
            }
        }
        else
        {
            var isSNPButtonShow = "1";
            var Url = "https://www.youtube.com/embed/2eyVxx1kih0?autoplay=1";
            var snpHeight = (window.innerHeight - 250);
            $("#snapshotDashboard").css({top:'0px'});
            $("#snapshotDashboard").dialog('option','minWidth',850);
            $("#snapshotDashboard").dialog('option','minHeight',snpHeight);
            $("#snapshotDashboard").dialog('option','resizable',false);
            $(".ui-dialog").css({top:'0px',position:'fixed'});
            $("#snpDemoPreview").show();
            $("#snapshotPreview").attr('height',(snpHeight-50));
            $("#snapshotPreview").attr('src',Url);
            $("#snapshotDashboard").dialog('open');
            
            if(isSNPButtonShow)
            {
                $("#snapshotDashboard").dialog('option','buttons',
                [
                    {
                        id: 'okSnpBtn',
                        text: "Yes, activate my FREE SnapShot Dashboard on Hotelogix",
                        click: function() {
                            if($("#signupAgree").is(":checked") == true)
                            {
                                var hotelMode = "TRIAL";
                                if(hotelMode != 'PAID')
                                {
                                    alert("To activate FREE SnapShot Analytics, please upgrade your account to a Premium plan. For more details, contact support@hotelogix.com.");
                                    return false;
                                }
                                else
                                {
                                    $(".ui-dialog").css({'z-index':'100'});
                                    oPanelLoadingDiv.show();
                                    $("#loadingPanelText").html("SnapShot Activation in Process....");
                                    $("#PanelModalWait_c").css({'z-index':'999'});
                                    $("#PanelModalWait_mask").css({'z-index':'101'});

                                    var actionUrl = 'https://12937.hotelogix.net/frontdesk/reservation/sendsnapshotactivation';
                                    $.ajax({
                                        url: actionUrl,
                                        type: "POST",
                                        data: {},
                                        error: function(jqXHR, exception) {
                                            oPanelLoadingDiv.hide();
                                            alert('Something went wrong. Please try again later.');

                                        },
                                        success: function(data) {
                                            $(".ui-dialog").css({zIndex:101});
                                            $("#PanelModalWait_mask").css({zIndex:91});
                                            $("#PanelModalWait_c").css({zIndex:92});
                                            oPanelLoadingDiv.hide();
                                            alert("SnapShot Activation Request is successfully sent.");
                                            $("#snpAgree").hide();
                                            $("#snpIna").html("Your request to activate SnapShot has been sent. The SnapShot activation is under process");
                                            $("#snpIna").show();
                                            $('#snapshotDashboard').siblings('.ui-dialog-buttonpane').hide();
                                            $("#snapshotDashboard").dialog("close");
                                        }
                                    });
                                }
                            }
                            else
                            {
                                alert("Please select `I Agree to Terms of Use and Service`.");
                                return false;
                            }
                        }
                    },
                    {
                        id: 'cancelSnpBtn',
                        text: "No, I don`t want this Analytics Dashboard",
                        click: function() {
                            $("#snapshotPreview").attr('src','');
                            $("#snpDemoPreview").hide();
                            $("#signupAgree").prop('checked',false);
                            $("#snapshotDashboard").dialog( "close" );
                        }
                    },
                    {
                        id: 'doNotShowAg',
                        text: "",
                        click: function () {
                            
                        }
                        
                    }
                ]);
                
                
                
                $('.ui-dialog-buttonset #doNotShowAg',$("#snapshotDashboard").parent()).replaceWith("<br><input type='checkbox' id='doNotShowAgain' name='doNotShowAgain'>Don't show this offer again!</input>");
                $("#doNotShowAgain").on('click', function() {
                    if($(this).is(":checked") == true)
                    {
                        if(confirm("Are you sure you don`t wish to use FREE SnapShot Dashboard?"))
                        {
                            
                            var actionUrl = 'https://12937.hotelogix.net/frontdesk/reservation/marksnpoffer';
                            $.ajax({
                                url: actionUrl,
                                type: "POST",
                                data: {isnotusesnp:1},
                                error: function(jqXHR, exception) {
                                   console.log("Snapshot Optin - Error in ajax when sending for do not show again!");
                                },
                                success: function(data) {
                                    $("#snapshotPreview").attr('src','');
                                    $("#snpDemoPreview").hide();
                                    $("#snapshotOptinLi").hide();
                                    $("#snapshotDashboard").dialog('destroy');
                                }
                            });
                        }
                        else
                        {
                            $(this).prop("checked",false);
                        }
                    }
                });
            }
            
            $("#snapshotDashboard").on("dialogbeforeclose", function( event, ui ) {
                    $("#snapshotPreview").attr('src','');
                    $("#snpDemoPreview").hide();
                    if($("#signupAgree"))
                    {
                        $("#signupAgree").prop('checked',false);
                    }
                });
        }
    });
    
    $( window ).resize(function() {
        var snpHeight = (window.innerHeight - 250);
        $("#snapshotDashboard").css({top:'0px'});
        $(".ui-dialog").css({top:'0px'});
        $("#snapshotDashboard").dialog('option','minHeight',snpHeight);
        $("#snapshotPreview").attr('height',(snpHeight-50));
    });
    

