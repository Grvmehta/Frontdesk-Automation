<script>
    
    
    
    
    var currentHid = "12937";
    var currentHotelName = "AkashDhaba";
    $(document).ready(function () {
        
        Frontdesk.ready({});
        
        $("#openHotels").autocomplete({
            minLength: 0,
            source: "https://12937.hotelogix.net/frontdesk/reservation/autocompleteproperty",
            create: function( event, ui ) {
                $("#openHotels").autocomplete("widget").attr('style', 'max-height: 150px; overflow-y: auto; overflow-x: hidden;');
            },
            
            response: function (event, ui) {
                console.log(ui);
            },
            select: function( event, ui ) 
            { 
                event.target.value = currentHotelName +" ("+currentHid+")";
                openproperty(ui.item,currentHid);
                return false;
            }
        });
    });
    
    $("#openHotels").on("blur",function () {
        $(this).val(currentHotelName +" ("+currentHid+")");
    });
</script>
<style>
    .ui-autocomplete li{
        font-size:10px;
        width:120px;
        overflow: hidden;
        white-space: nowrap;
        text-overflow: ellipsis;
