<script>
function printinvoicesummary(groupId,rsvId,custmFolioNo,invoiceNo,printFolioFunction)
{
    
        var action = "https://12937.hotelogix.net/frontdesk/reservation/audittrailforinvsmry/";
        var postData = {
        "groupId":groupId,
        "rsvId":rsvId,
        "custmFolioNo":custmFolioNo,
        "invoiceNo":invoiceNo
            };
       
        $.ajax({
            url: action,
            type:"POST",
            dataType: "json",
            data: postData,
            error: function(error)
            {
             console.log(error); 
             alert(data.errorMsg);
            },
            success: function( data ) {
                console.log(data);
                if (data.isError == 0)
                {
                    eval(printFolioFunction);
                }
                else
                {
                    alert(data.errorMsg);
                    return false;
                }
                
            }
        });
    
}
</script>
