  100% {
    -webkit-transform: rotate(360deg);
    transform: rotate(360deg);
  }
}

/* close button */

.ss-survey-launch__close-button {
  position: absolute;
  right: 0;
  top: 0;
  z-index: 2;
  line-height: 1;
  cursor: pointer;
  -webkit-user-select: none;
  -moz-user-select: none;
  -ms-user-select: none;
  user-select: none;
  background-color: transparent;
  outline: none;
  -webkit-box-shadow: none;
  box-shadow: none;
  border: 0;
  margin: 0;
  padding: 8px;
  -webkit-transition: opacity 0.3s;
  -o-transition: opacity 0.3s;
  transition: opacity 0.3s;
}

.ss-survey-launch__close-button:hover {
  opacity: 0.8;
}

.ss-survey-launch__close-button svg {
  float: left;
}

.ss-survey-launch__backdrop {
  background-color: rgba(0, 0, 0, 0.9);
  position: absolute;
  left: 0;
  top: 0;
  width: 100%;
  height: 100%;
}

