<!-- 
@description - client site validation for the mandatory field setting 
@param - mandatoryFieldsObj
@param - frmEvent (RESERVATION OR CHECKIN)
@param frm (Form Object)
-->
<script>                                                                                                                     
  var validateMandatoryFormFields = function(mandatoryFieldsObj,frmEvent,frm)
    {
        var fieldSettingsObj = {};
        var fieldsKeyLabelObj = {}; 
        var returnStatus = true;
        if(!($.isEmptyObject(fieldSettingsObj)))
        { 
            if(frmEvent in fieldSettingsObj)
            {
              var savedFields = fieldSettingsObj[frmEvent];
              if( savedFields !=='' && savedFields !== undefined )
                {
                    var savedFieldsArr = savedFields.replace(/ /g,'').split(',');
                    for (var x = 0; x < savedFieldsArr.length; x++)
                    {
                        var fieldKey = savedFieldsArr[x];
                        if(fieldKey in mandatoryFieldsObj)
                        {
                            // in case, a field has multiple subfields to be validated(eg. dob have day, month, year sub fields) 
                            var fieldNamesArr  = mandatoryFieldsObj[fieldKey].replace(/ /g,'').split(',');
                            var fieldKeyArr = fieldKey.split('_');
                            if(fieldKeyArr[0] != 'identityType')
                            {
                                for(var z = 0; z < fieldNamesArr.length; z++)
                                {
                                    var fieldName = fieldNamesArr[z];
                                    if(frm[fieldName])
                                    {
                                        if( !(frm[fieldName].value.trim()) || (frm[fieldName].value.trim() == '0') 
                                            )
                                        {
                                            console.log(fieldKey);
                                            console.log(fieldsKeyLabelObj);
                                            alert(jsformatstr(lang_mi_11735_5, fieldsKeyLabelObj[fieldKey], frmEvent.toLowerCase()));
                                            frm[fieldName].focus();    
                                            returnStatus = false;
                                            return false;
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
        return returnStatus;

    }; 
       
    //loader inside sparrow button
            var isSparrowFeedbackOpen = false;
        var spLoaderStatus = false;
         function clic(){
             var sploader = $("#sparrowFeedback");
            satismeter('track', { event: 'clicked()' });
            sploader.addClass("spBtnLoader");
            setTimeout(function(){
            sploader.removeClass("spBtnLoader");
         }, 2500);
            
//end of loader function
         }
        
         $(document).ready(function(){            
            
            $(".arrow.slideSparrowFeedback").click(function(){

                if(isSparrowFeedbackOpen)
                {
                    $('.slideSparrowFeedback').html('&#8249;');//< symbol
                    $("#sparrowFeedback").animate({ "right": "-191px" }, "slow" ); //on  
                    isSparrowFeedbackOpen=false;
                }else
                {
                    $('.slideSparrowFeedback').html('&#8250;');//> symbol
                    $("#sparrowFeedback").animate({ "right": "0" }, "slow" );
                    isSparrowFeedbackOpen=true;
                }
            });//End
                    
        });
    </script>

