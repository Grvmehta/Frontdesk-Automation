<script language="javascript">
function Navigate(o) {
	if( parseInt(document.frmListView.maxEntries.value)>=100000){
		if(!confirm('You have selected to view all records on a single page.\nIt may take some time to show all the records.\nIf the number of records are too much then it may misconfigure your server.\nDo you want to continue?')){
			return false;
		}
	}
    
    	document.frmListView.page.value = o;
	document.frmListView.offset.value = o;
	document.frmListView.submit();
	
}

function submitForm() {
	document.frmListView.submit();
}

function submitForm4Rmtype() {
	document.frmListView.page.value = 0;
	document.frmListView.offset.value = 0;
	document.frmListView.submit();
	
}

function doSort(fld, order_by) {
	document.frmListView.orderBy.value = fld;
	document.frmListView.sortOrder.value = order_by;
	document.frmListView.submit();
}

function toggleSelect(fl) {
	var i = 5;
	for(j=1; j <= i; j++) {
            if(document.getElementById("id_"+j))
            {
		document.getElementById("id_"+j).checked = fl;
            }
	}
        
        if( $("#POSCATStbl").length )
        {
             $("#POSCATStbl").hide(); 
        }
}
function addNew(act) {
	toggleSelect(false);
	if(!act) {
		document.frmListView.action = "https://20803.staygrid.com/admine/packagedetails/editview/attamp/1";
	}else {
		document.frmListView.action = "https://20803.staygrid.com/admine/packagedetails/"+act;
	}
	document.frmListView.submit();
	
}

function addPayMaster(act)
{
    toggleSelect(false);
    if(!act) 
    {
        document.frmListView.action = "https://20803.staygrid.com/admine/packagedetails/editview/attamp/1";
    }else 
    {
       document.frmListView.action = "https://20803.staygrid.com/admine/packagedetails/"+act;
    }
    document.frmListView.type.value = 'PM';
    document.frmListView.submit(); 
}

function addNewGstin(act){
	toggleSelect(false);
	if(!act) {
		document.frmListView.action = "https://20803.staygrid.com/admine/packagedetails/editview";
	}else {
		document.frmListView.action = "https://20803.staygrid.com/admine/packagedetails/"+act;
	}
	document.frmListView.submit();	
}
	function editMultiple(act,totalCtrl) {
		var i =0;
		var j = 0;
		for(i = 1; i <= totalCtrl; ++i) {
			chkBox = document.getElementById("id_"+i);
			if(chkBox.checked == true) {	
				j++;
			}
		}
		if (j == 0) return false;
		
		document.frmListView.action = "https://20803.staygrid.com/admine/packagedetails/"+act;
		document.frmListView.submit();
	}

	function deleteSelected(totalCtrl,scope) {
        
        var actionName = "delete";
        if(arguments[2])
        {
            actionName  = arguments[2];            
        }
        
		var i =0;
		var j = 0;
		
	
		for(i = 1; i <= totalCtrl; ++i) {
		
			if(document.getElementById("id_"+i))
			{
				chkBox = document.getElementById("id_"+i);
				if(chkBox.checked == true) {	
					j++;
				}
			}
		}
		
		if(j > 0) {
		
					
			if(!confirm("Do you really want to delete the selected records?")) {
				return 0;
			}
		}
		
		if (j == 0) return;
               
                if(scope!=null)
                {
                    var scope = '/scope/'+scope;
                }
                else
                {
                     var scope = '';
                }
			document.frmListView.action = "https://20803.staygrid.com/admine/packagedetails/"+actionName+scope;
			document.frmListView.submit();
	}
	
	function logoutSelected(totalCtrl) {
		var i =0;
		var j = 0;
		
	
		for(i = 1; i <= totalCtrl; ++i) {
			chkBox = document.getElementById("id_"+i);
			if(chkBox.checked == true) {	
				j++;
			}
		}
		
		if(j > 0) {
			if(!confirm("Do you really wish to Logout the user(s)?")) {
				return 0;
			}
		}
		
		if (j ==0) return;
			document.frmListView.action = "https://20803.staygrid.com/admine/packagedetails/delete";
			document.frmListView.submit();
	}
  
	function changStatus(id) {
        
        var actionName = "changestatus";
        if(arguments[1])
        {
            actionName  = arguments[1];            
        }
        
		var imgUrl =  "https://20803.staygrid.com/images/";
		var imgDiv = document.getElementById("status_"+id);
        var prevImg = imgDiv.src;
		imgDiv.src = waitLoader.src;
		var handleSuccess = function(o){
			if(o.responseText !== undefined){
				if(o.responseText=='accessDenied'){
					window.location.href="https://20803.staygrid.com/admine/unauthorized/noper/";
				}else{
					
					var data =o.responseText.split('||');
					var data1 =  data[0];
					var data2 =  data[1];
                    /* 
                    #201301311355:ankit #3651  If we delete Pos tax from POS it keeps showing up in POS point  
                    */
                    var statusAtt = '';
                    
                    if(imgDiv.hasAttribute('statusAtt'))
                    {
                        if(o.responseText == 'on.GIF')
                        {
                            imgDiv.setAttribute('statusAtt','A');
                        }else if(o.responseText == 'off.GIF')
                        {
                            imgDiv.setAttribute('statusAtt','I');
                        }
                    }
                    
					if( o.responseText=='cantdeleterm')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("This Room cannot be deactivated as it has Active Reservations/DNR");

					}
					else if( o.responseText=='cantdeletekitchen')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("This kitchen cannot be deactivated as it is attached with the POS Product.");

					}
                    					else if( o.responseText=='cantdeletermatg')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("This room is pulblished on GDS. Status cannot be changed.");

					}
					else if( o.responseText=='cantdeletermt')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("This Room Type cannot be deactivated as it has Active Reservations/DNR");

					}
                    					else if( o.responseText=='cantdeletermtatg')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("Room Type is attached to GDS. Status cannot be changed.");

					}
					else if( o.responseText=='cantdeletepos')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("This POS Point cannot be deactivated as it is attached with the inclusion(s).");

					}
					else if( o.responseText=='cantdeletepospro')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("This product cannot be deactivated as it is attached with the inclusion(s).");

					}
					else if( o.responseText=='cantdeleteseaatt')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("This sesson attribute cannot be deactivated as it is attached with the season(s).");

					}
					else if( o.responseText=='cantdeletespecialp')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("This Special Period cannot be deactivated as it is attached with the package(s).");

					}
					else if( data1=='languagecannotactivate')
					{
						imgDiv.src =imgUrl + "off.GIF";
						MM_openBrWindow('https://20803.staygrid.com/admine/packagedetails/popup/languageId/'+data2,'langActive','scrollbars=yes,width=500,height=500,resizable=3');

					}else if( o.responseText=='cantdeletecounter')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("You cannot deactivate the selected counter as it is actively used for exchange of transactions.");

					}
					else if( o.responseText=='cantactiveamountslabtax')
					{
						imgDiv.src =imgUrl + "off.GIF";
						alert("Slab tax with a range cannot be activated if there are inclusive of tax Packages present in the system.\n Please keep the range blank or delete the inclusive of Tax packages to proceed.");

					}
                                        else if( o.responseText=='candeletepaytype')
					{
                        if(prevImg == imgUrl + "on.GIF")
                        {
                            imgDiv.src =imgUrl + "on.GIF";
                        }else
                        {
                            imgDiv.src =imgUrl + "off.GIF";
                        }
						alert("You cannot activate/deactivate the selected Pay type(s) as it is actively used for Open Counter.");

					}
					
                                         else if( o.responseText=='cantchangeuserlevelstatus')
					{
                        if(prevImg == imgUrl + "on.GIF")
                        {
                            imgDiv.src =imgUrl + "on.GIF";
                        }else
                        {
                            imgDiv.src =imgUrl + "off.GIF";
                        }
						alert("You cannot deactivate the selected user level because this user level has active users.");

					}
					else if( o.responseText=='cantactiveeb')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("Selected discount can not be deactivated as it is part of package discount/default package discount.");

					}
					else if( o.responseText=='tagcantdeactivate')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("Selected tag can not be deactivated as it is used for Reservation/Group.");

					}else if( o.responseText=='jobtypecantdeactivate')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("Selected Job Type can not be deactivated as it is used for Maintenance module.");
                                        }
					else if( o.responseText=='discreasoncantdeactivate')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("Selected discount reason can not be deactivated as it is used for Reservation/Group.");

					}
                    else if( o.responseText=='otherhotelareacantdeactivate')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("Selected Other Hotel Area can not be deactivated as it is used for Maintenance module.");
                    }
                    else if( o.responseText=='fieldcantactivate')
					{
                                            imgDiv.src =imgUrl + "off.GIF";
                                            alert("Active custom field(s) can not be more than 10.");

					}
                    else if( o.responseText=='fieldcantdeactivate')
					{
                                            imgDiv.src =imgUrl + "on.GIF";
                                            alert("Selected custom field can not be deactivated as it is used for Reservation/Group.");

					}
                                        else if( o.responseText=='mappedPOSISTTax')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("Selected POS Tax cannot be deactivated as it is mapped to POSIST POS");
                                        }
                                        else if( o.responseText=='mappedPOSISTPayType')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("Selected Pay Type cannot be deactivated as it is mapped to POSIST POS");
                                        }
                                        else if( o.responseText=='mappedPOSISTPosCat')
					{
						imgDiv.src =imgUrl + "on.GIF";
						alert("Selected POS Category cannot be deactivated as it is mapped to POSIST POS");
                                        }
                                        
					else
					{
						imgDiv.src =imgUrl + o.responseText;
						return o.responseText;
					}
				}
			}
		}
		var handleFailure = function(o){
			if(o.responseText !== undefined){
				return "Operation Failed";
			}
		}
		var callback={
			success:handleSuccess,
			failure: handleFailure,
			argument: { foo:"foo", bar:"bar" }
		};
		
		sUrl = 'https://20803.staygrid.com/admine/packagedetails/'+actionName+'/id/'+id;
		
		var request = YAHOO.util.Connect.asyncRequest('GET', sUrl, callback);
	}
	function  changStatusP(id,scope,posid)
     {
			if(!confirm("This action will reset the series by adding a prefix. Once the series# is reset, you will not be able to switch it back to old series. Do you wish to continue?")) {
				return false;
			}
            $.ajax({
            url: "https://20803.staygrid.com/admine/packagedetails/resetprefix",
            type: "post",
			data: {id:id, scope:scope,posid:posid},			
            success: function (response) 
            {
				var dataArray = $.parseJSON(response);
				if(dataArray.isError==0){				
					$('#prefix-'+scope+'-'+id).html(dataArray.data); 
					$('.error').html(dataArray.message); 
				}else{
					$('.error').html(dataArray.message);
				}
            }
            });
           
    }

function changePrecisionAction(id, pricision) {
		var handleSuccess = function(o){ 
			if(o.responseText=='accessDenied'){
				window.location.href="https://20803.staygrid.com/admine/unauthorized/noper/";
			}else{
				return o.responseText;
			}
		}
		var handleFailure = function(o){ 
			if(o.responseText !== undefined){ 
				return "Operation Failed";
			} 
		}
		var callback = { 
			success:handleSuccess, 
			failure: handleFailure, 
			argument: { foo:"foo", bar:"bar" } 
		};
		sUrl = 'https://20803.staygrid.com/admine/packagedetails/changePrecision/id/'+id+'/pricision/'+pricision+'/';
		var request = YAHOO.util.Connect.asyncRequest('GET', sUrl, callback); 
}



function deleteSelected_v2(totalCtrl) {
				
		
		var frm = document.frmListView;
		var chkBox = null;
		
		var idObject = new Array();
		var j = 0;
		for(i = 1; i <= totalCtrl; ++i) {
			chkBox = document.getElementById("id_"+i);
			if(chkBox.checked == true) {	
				j++;
			}
		}
		
		if(j > 0) {
			if(!confirm("Do you really want to delete the selected records?")) {
				return 0;
			}
		}
		
		if (j ==0) return;
		j = 0;
		YAHOO.example.container.wait.show();

		for(i = 1; i <= totalCtrl; ++i) {
			chkBox = document.getElementById("id_"+i);
			
			if(chkBox.checked == true) {
				idObject[j] = chkBox.value;
				++j;
				chkBox.checked = false;
			}
		}
		
		
		var handleSuccess = function(o) { 
			var idArray = []; 
			if(o.responseText !== undefined){ 
				try { 
					idArray = YAHOO.lang.JSON.parse(o.responseText); 
				} 
				catch (x) { 
					alert("JSON Parse failed!"); 
						YAHOO.example.container.wait.hide();
					return; 
				}
				 for (var i = 0, len = idArray.length; i < len; ++i) { 
					 	YAHOO.util.Dom.setStyle("row_"+idArray[i], "display" , "none");
					 	var rowObj = YAHOO.util.Dom.get("row_"+ idArray[i]);
					 	colObj = YAHOO.util.Dom.getFirstChild("row_"+ idArray[i]);
					 	colObj.innerHTML = 0;
					 	
					 	
				 }
				 var sn = 1;
				 for(i  = 1; i <=totalCtrl; ++i) {
					 Obj = YAHOO.util.Dom.get("col_"+ i);
					 
					 var num = parseInt(Obj.innerHTML);
					 if(num != 0) {
						 Obj.innerHTML = sn;
						 ++sn;
					 }
					 
				 }
				 
				YAHOO.example.container.wait.hide();
			} 
		}
		
		var handleFailure = function(o){ 
			if(o.responseText !== undefined){ 
				YAHOO.example.container.wait.hide();
				return "Operation Failed";
			} 
		}
		var callback = { 
			success:handleSuccess, 
			failure: handleFailure, 
			argument: { foo:"foo", bar:"bar" } 
		}; 	
	
		sUrl = 'https://20803.staygrid.com/admine/packagedetails/delete';
		postData = "ids="+idObject;
		var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback, postData); 
		
}


function MM_goToURL() { //v3.0
  var i, args=MM_goToURL.arguments; document.MM_returnValue = false;
  for (i=0; i<(args.length-1); i+=2) eval(args[i]+".location='"+args[i+1]+"'");
}
function MM_openBrWindow(theURL,winName,features) { //v2.0
  mychild = window.open(theURL,winName,features);
  mychild.focus();
}

function calDates(sId, newVal, curDayVal, curMonthVal, eId, currId, firstId, lastId, setValToControl ) {
	
	var daysDom=' ';
	var monthsDom=' ';
	var daysInMonthListDom=' ';
	var daysDom=daysDom.split(",");
	monthsDom=monthsDom.split(",");
	daysInMonthListDom=daysInMonthListDom.split(",");
	
	var sDays=YAHOO.util.Dom.get(sId);
	var sMonths=YAHOO.util.Dom.get("startMonths_"+ parseInt(currId));
	
	//alert(" curr id ="+currId);
	
	//alert(" lastId id ="+lastId);

	var newVal=newVal;
	var newValMonth;
	//alert(newVal);
	if (parseInt(daysInMonthListDom[sMonths.value-1]) < parseInt( newVal)) {
		alert("Not a valid Date");
		sDays.value=curDayVal;
		return false;
	}
	if(parseInt(currId)==parseInt(firstId)) {
		var x=parseInt(lastId);
		var eDate=YAHOO.util.Dom.get("endDatDay_"+parseInt(x));
		var endDays=YAHOO.util.Dom.get("endDays_"+parseInt(x));
		var endMonths=YAHOO.util.Dom.get("endMonths_"+parseInt(x));
	}
	if(parseInt(currId)==parseInt(lastId)) {
		var x=parseInt(currId)-1;
		if (x<=0 ) x=1;
		var eDate=YAHOO.util.Dom.get("endDatDay_"+parseInt(x));
		var endDays=YAHOO.util.Dom.get("endDays_"+parseInt(x));
		var endMonths=YAHOO.util.Dom.get("endMonths_"+parseInt(x));
	}
	
	if(parseInt(currId)!=parseInt(lastId) && parseInt(currId)!=parseInt(firstId) ) {
		var x=parseInt(currId)-1;
		if (x<=0 ) x=1;
		//alert(x);
		var eDate=YAHOO.util.Dom.get("endDatDay_"+parseInt(x));
		var endDays=YAHOO.util.Dom.get("endDays_"+parseInt(x));
		var endMonths=YAHOO.util.Dom.get("endMonths_"+parseInt(x));
	}

	
	var nDateStr="";
	
	if(newVal==1) {
		if (sMonths.value==1) {
			newVal=daysInMonthListDom[monthsDom.length -1];
			nDateStr = newVal+ ' ' + monthsDom[monthsDom.length -1];
			//endDays.value=newVal;
			//endMonths.value=monthsDom.length;
			newValMonth=monthsDom.length;
		} else {
			newVal=daysInMonthListDom[sMonths.value-2];
			newVal=parseInt(daysInMonthListDom[sMonths.value-2]);
			nDateStr = newVal+ ' ' + monthsDom[parseInt(curMonthVal)-2];
			//endDays.value=newVal;
			//endMonths.value=parseInt(curMonthVal)-1;
			newValMonth=parseInt(curMonthVal)-1;
		}
	}else {
		newVal=parseInt(newVal)-1;
		nDateStr = newVal+ ' ' + monthsDom[sMonths.value-1];
		newValMonth=parseInt(sMonths.value);
	}
	
	if (setValToControl==1) {
		endDays.value=newVal;
		endMonths.value=newValMonth;
		eDate.innerHTML=nDateStr;
	} else {
		return endDays.value +", "+endMonths.value
	}
}

function showHideElement(elemName, spanId)
{
    var elems = document.getElementsByName(elemName);
    if(elems.length > 0)
    {
        for(i=0;i < elems.length;i++)
        {
            if(elems[i].style.display == '')
            {
                elems[i].style.display = 'none';
                document.getElementById(spanId).innerHTML = "&plus;";

            }
            else if(elems[i].style.display == 'none')
            {
                elems[i].style.display = '';        
                document.getElementById(spanId).innerHTML = "&minus;";
            }
        }
    }
}

