<script>
    var forMail = '';
    var previewmail = '';
    if (previewmail == 1 || forMail == 1) {
        document.getElementById('dont-email').style.display = 'none';
    }

    
    function showResCMCurr(id) {
      eval("var lnk = showResCMLink_" + id);
      eval("var cmb = cmResCurrSel_" + id);
      $(lnk).css('display', 'none');
      $(cmb).css('display', 'inline');
      cmb.focus();
    }

    function switchResCMCurr(id) {
      eval("var lnk = showResCMLink_" + id);
      eval("var cmb = cmResCurrSel_" + id);
      $(lnk).css('display', 'inline');
      $(cmb).css('display', 'none');
      cmb.focus();
    }

    function changeSingleResPkgCurr(id, rid) {
      var lnk = document.getElementById("showResCMLink_" + id);
      var cmb = document.getElementById("cmResCurrSel_" + id);
      var mailCurr = cmb.options[cmb.selectedIndex].value;
      if (!mailCurr) {
        mailCurr = 0;
      }
      $('#hiddenOthPkgVal').val(mailCurr);
      $('#ishidePrice').val(1);
      $('#hidePriceReq').val(2);
      $('#signature_formId').submit();
    }
    function btnShowHidePrice(){
      $('#ishidePrice').val(1);
    }
</script>
