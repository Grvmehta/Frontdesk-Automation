function deletePackageImage(id,container) {
	
	
	YAHOO.util.Dom.setStyle(container,'background','#ff0000');
	
	if(!confirm('Do you wish to delete the selected toggleTextimage?')) {
		YAHOO.util.Dom.setStyle(container,'background','#ffffff');
		return false;
	}
	
	
	var handleSuccess = function(o){ 
	
	
		if(o.responseText == 1) {
			YAHOO.util.Dom.setStyle(container,'display','none');
		}
		if(o.responseText == 0) {toggleText
			alert("Image could not Delete!");
		}
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	

	var sUrl = 'https://25273.staygrid.com/V2/admine/packagedetails/imagedelete/id/'+id;
	
	var request = YAHOO.util.Connect.asyncRequest('GET', sUrl, callback); 
	return false;

}

function changeMarkupSelect(obj,pkgId,validityId,from)
{
    if(obj.value == 4)
    {
        YAHOO.util.Dom.setStyle("dvFixedMargin",'display','block');

    }else{
    
        YAHOO.util.Dom.setStyle("dvFixedMargin",'display','none');
        YAHOO.util.Dom.get('txtFixedMargin').value = "0.00";
        updateAgentComm(pkgId,validityId,from);
    }
}
