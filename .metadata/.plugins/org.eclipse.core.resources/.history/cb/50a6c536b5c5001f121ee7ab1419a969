// #201404291400 #3658:sougata alert before ptg
// #201406101400 #5282:sougata remove inclusive of tax custom settings
function publishToGridPreview(obj,pkgPostingType,mode)
{
    var go = true;
    
    var postInclTax = document.getElementById('postInclusiveTax');
    var sentInclTax = document.getElementById('inclusiveOfTax');
    //#201502101400 #5423:sougata no need now
    /*if(postInclTax)
    {
        
        if(sentInclTax)
        {
            var postInclTaxChk = postInclTax.checked;
            var sentInclTaxChk = sentInclTax.checked;
            if(postInclTaxChk != sentInclTaxChk)
            {
                if(mode == 'W')
                {
                    alert("The taxes set up for package rate and sent rate need to be same to publish on grid.");
                }
                else if(mode == 'D')
                {
                    alert("The taxes set up for package rate and sent rate need to be same to publish on daily rate.");
                }
                go = false;
                return;
            }
        }
    }
    else if(sentInclTax && sentInclTax.checked)
    {
        if(mode == 'W')
        {
            alert("The taxes set up for package rate and sent rate need to be same to publish on grid.");
        }
        else if(mode == 'D')
        {
            alert("The taxes set up for package rate and sent rate need to be same to publish on daily rate.");
        }
                
        
        go = false;
        return;
    }*/
    if(pkgPostingType === pkgPostingTypeMonthly)
    {
        var valSurchs = $(document).find("input.chkSurChCls");
        valSurchs.map(function(i, el)
        {
            if($(el).val() > 0)
            {
                alert('Weekday surcharge do not work along with monthly grid rates. Please remove week day surcharge and then save the rates.');
                go = false;
                return;
            }
        });
    }
    
    
    if(go)
    {
        obj.disabled = true;
        var confText = '';
        if(mode == 'W')
        {
            confText = 'Any changes made to the package will be saved automatically and redirect to the grid preview page.\n Would you like to proceed?'
        }
        else if(mode == 'D')
        {
           confText = 'Any changes made to the package will be saved automatically and redirect to the daily rate preview page.\n Would you like to proceed?'
        }
        
        if(confirm(confText)) {
             
                var  frm = document.getElementById('frmEditView');
                if(frmChk(frm,mode))
                {
                    frm.submit();
                }
             
             }
        }
     obj.disabled = false;
   
}

//#201405121400 #3658:sougata changes text
//#201405121600 #3658:sougata changes text
function publishToDailyGridPreview(obj,pkgPostingType)
{
   publishToGridPreview(obj,pkgPostingType,'D');
   
}

function doSaveNResync()
{
    document.getElementById('frmEditView').doSaveAndReSync.value=1;
    if(frmChk(document.getElementById('frmEditView'),''))
    {
        document.getElementById('frmEditView').submit();
    }
}


$(document).ready(function() {    
     
        var allotMt = new allotmentMatrix();     
        allotMt.ready({
               refId : '-1',
               refType : 'fdesk',
               baseUrl : 'https://25273.staygrid.com/V2',
               matrixStartDate : '',                              
               rateId : '',
               isReadOnlyAllotMatrix: ''
        }); 
        
        $(".link-ViewAllotGridInPkg").click(function(){           
            var dt = $(this).attr('date');
            var state = $(this).attr('state');
            
            if(state === 'open')
            {
                $(this).html('View');
                $("#dvAllotmentMatrix").html('');
                $("#dvAllotmentMatrix").hide();                
                $(this).removeAttr('state');
            }
            else
            {   
                $(this).html('Hide');
                $(this).attr('state', 'open');
                $("#dvAllotmentMatrix").show();
                allotMt.loadMatrixData(dt);   
                
            }
        });
        
        $(".link-editRestPkg").click(function(){           
            var dt = $(this).attr('date');
            window.open('https://25273.staygrid.com/V2/admine/allotmentroom/listview/refId/-1/refType/fdesk/currDt/'+dt+'/agentId//rateId/');                        
            
        });
        

        $("#tooltipCon").tooltip();
        $('#tooltipCon').tooltip('option', 'position', { my: 'center bottom', at: 'center top-10',collision:'none',within:"table" });
        $('#tooltipCon').tooltip('option', 'tooltipClass', "top");        
 });
 
 function askForMaxSyncDate(frm,previewMode)
 {
    if(pkgType == 'N' && pkgStatus =='A' &&  isSyncToChannelAllow == '1' && syncOff != '1')
    {
        if(isCentRate === "1")
        {       
            if('0' === "1")
            {
                alert("Centralized rate Sync date is past current date.\n Please set the sync date from centralized rate.");
            }
            else if($("#maxSyncDate").val() == '' && ''!='')// for first time when child itself going for sync
            {
                alert(jsformatstr("Sync request will be sent upto %s1","Nov 30, -1"));
            }
            $("#maxSyncDate").val(''); // set parent max sync date.
            
            if($("#maxSyncDate").val() == '')
            {
                alert("Sync date is not setup. Please set the sync date from centralized rate.");
            }
            
            frm.clickPubToGrd.value = previewMode;
            frm.submit();
            
            return false;
        }
        
        var askBox = $("#askMaxSncDt");
        askBox.dialog({
                        modal:true
                        ,width:280
                        ,minHeight:29
                        ,create: function( event, ui ) {

                        }
                        ,open: function( event, ui ) {

                           // ui-dialog-titlebar ui-widget-header ui-corner-all ui-helper-clearfix ui-draggable-handle
                           $(".ui-dialog-titlebar").hide();
                           JQcalendar($("#maxSyncDateShow"));

                        },close: function( event, ui ) {
                            $("#maxSyncDateShow").val($("#maxSyncDateShow").attr("oldValue"))
                            $("#maxSyncDateShow").datepicker("destroy");
                        }
                        ,buttons:[ 
                                    { 
                                        id: "btnDlg-sync"
                                        ,text: "Save & Sync"
                                        ,click: function() 
                                        { 
                                            if($("#maxSyncDate").val() == '')
                                            {
                                                alert("Please select date.");
                                                return false;
                                            }
                                            $("#btnDlg-sync").html('<span class="ui-button-text">Syncing...</span>');
                                            $("#btnDlg-sync").attr('disabled', true);
                                            $("#btnDlg-close").attr('disabled', true);
                                            frm.clickPubToGrd.value = previewMode;
                                            frm.submit();
                                        }
                                    } 
                                    ,{ 
                                        id: "btnDlg-close"
                                        ,text: "Cancel"
                                        ,click: function() 
                                        { 
                                            $( this ).dialog( "close" ); 
                                        }
                                    } 
                                  ]
                      }).dialog('open');

            return false;
    }
    else
    {
    
      frm.clickPubToGrd.value = previewMode;
      return true;
    }
                
                
 }
 

 
function JQDatepickerOnselect(eleObj,selected,onselectCallBack)
{
    if (onselectCallBack && typeof onselectCallBack == 'function')            { 
                onselectCallBack(); 
    }

    if (eleObj.hasClass("fromDate"))
    {
        var minDate = new Date(selected);
        if (minDate) {
            if(eleObj.hasClass("sameDate"))
            {
                minDate.setDate(minDate.getDate());
            }else
            {
                minDate.setDate(minDate.getDate() + 1);
            }
        }
        eleObj.closest("tr").find(".toDate").datepicker("option", "minDate", minDate);
    }
    else if (eleObj.hasClass("toDate"))
    {
        var maxDate = new Date(selected);
        if (maxDate) {
            if(eleObj.hasClass("sameDate"))
            {
               maxDate.setDate(maxDate.getDate());
            }else
            {
                maxDate.setDate(maxDate.getDate() - 1);
            }
            
            
        }
        //eleObj.closest("tr").find(".fromDate").datepicker("option", "maxDate", maxDate);

    }
}

function JQcalendar(eleObj,onselectCallBack)
{
    var options = {
        dateFormat: eleObj.attr('dateFormat')?eleObj.attr('dateFormat'):"yy-mm-dd",
        changeMonth: true,
        changeYear: true,
        minDate: eleObj.attr('minDate'), 
        maxDate: eleObj.attr('maxDate'),
        onSelect: function(selected) {

            JQDatepickerOnselect(eleObj,selected,onselectCallBack);
        }
        
    };
    
    eleObj.datepicker(options);
   
    if(eleObj.attr('useAltField') && eleObj.attr('useAltField').length>0)
    {
        eleObj.datepicker( 'option' , 'altField', "#"+eleObj.attr('useAltField') );
        eleObj.datepicker( 'option' , 'altFormat', "yy-mm-dd" );
    }
    
    if (!eleObj.hasClass("dontShowIcon"))
    {
       eleObj.datepicker( 'option' , 'showOn', "button" );
       eleObj.datepicker( 'option' , 'buttonImage', "https://25273.staygrid.com/V2/images/calendar.gif" );
       eleObj.datepicker( 'option' , 'buttonImageOnly', true );
    }
}

/**
 * function is called when day use checkbox changes
 * @param {type} obj
 * @param {type} pkgId
 * @returns {undefined} 
 */
function dayUseChange(obj,pkgId)
{
    var isDayUse = (obj.checked) ? 1:0;
    var frm = document.getElementById('frmEditView');
    if(frm.minStay && frm.minStay.value > 1 && obj.checked == true)
    {
        alert("Package can not be marked as Day Use.");
        obj.checked = false;
        return;
    }
    
    var handleSuccess = function(o){
            if(o.responseText !== undefined)
            {
                if(o.responseText == pkgId)
                {
                    window.location.href='https://25273.staygrid.com/V2/admine/packagedetails/editview/id/'+pkgId;
                }else
                {
                    alert("Error in Day Use.");
                }
            }
	}
	var handleFailure = function(o){ 
            if(o.responseText !== undefined){ 
                    return "Operation Failed";
            } 
	}
	var callback = { 
            success:handleSuccess, 
            failure: handleFailure, 
            argument: { foo:"foo", bar:"bar"} 
	};
        
    sUrl = 'https://25273.staygrid.com/V2/admine/packagedetails/changedayuse/';
	var postVar='pkgId='+pkgId+'&isDayUse='+ isDayUse;
	var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback,postVar); 
}
