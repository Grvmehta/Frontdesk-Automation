function updateSyncOff(pkgId,doNOtAllowSync) {
        
	var so = (document.getElementById('syncOff').checked === true)?1:0;
        
        if(doNOtAllowSync == 1 && so == 0)
        {
            var message = "This Package or its Parent is Mapped with external rates From a Channel , So You Cannot Sync It";
            alert(message);
            return FALSE;
        }
        
	var handleSuccess = function(o){ 
		if(o.responseText !== undefined){ 
			try { 
                    window.location.href='https://25273.staygrid.com/V2/admine/packagedetails/editview/id/'+pkgId+"/"+Math.random()+"/";	
				
			}catch (x) { 
				alert("JSON Parse failed!"); 
				YAHOO.example.container.wait.hide();
				return; 
			}
		}
	
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	
	
    sUrl = 'https://25273.staygrid.com/V2/admine/packagedetails/updatesyncoff';	
		
	var postValue = 'pkgId='+pkgId+'&so='+so;		
	
	var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback,postValue);
       
}


