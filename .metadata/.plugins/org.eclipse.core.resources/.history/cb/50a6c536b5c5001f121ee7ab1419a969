function togglePriceAutoUpdate(vl) {
	var autoUpdatePrice = document.getElementById("autoUpdatePrice");
	YAHOO.util.Dom.setStyle("autoUpdatePrice","display","none");
	var autoUpdateLbl = document.getElementById("autoUpdateLbl");
	if(autoUpdatePrice.value == "") {
		autoUpdatePrice.value = 0;	
	}
	if(vl == 0) {
		autoUpdateLbl.innerHTML = "The Package Price will not change on increase in price of any of the component.";
	}else {
		autoUpdateLbl.innerHTML = "The Package Price will change on increase in price of any of the component.";
	}
	
	 updateOnAjax('autoUpdatePrice','autoUpdatePrice','','pck','','');
	 
	return false;
}
function toggleAutoUpdateWithTax(vl) {
	var autoUpdatePrice = document.getElementById("autoUpdateWithTax");
	YAHOO.util.Dom.setStyle("autoUpdateWithTax","display","none");
	var autoUpdateLbl = document.getElementById("autoUpdateWithTaxLbl");
	if(autoUpdatePrice.value == "") {
		autoUpdatePrice.value = 0;	
	}
	if(vl == 1) {
		autoUpdateLbl.innerHTML = "The Package Price will change on tax change.";
	}else {
		autoUpdateLbl.innerHTML = "The Package Price will not change on tax change.";
	}
	
	 updateOnAjax('autoUpdateWithTax','autoUpdateWithTax','','pck','','');
	 
	return false;
}


function toggleCaption(ctrlTxt, ctrl, vl, str1, str0) {
	var autoUpdatePrice = document.getElementById(ctrl);
	YAHOO.util.Dom.setStyle(ctrl, "display","none");
	var autoUpdateLbl = document.getElementById(ctrlTxt);
	if(vl == 0) {
		autoUpdateLbl.innerHTML = str0;
	}else {
		autoUpdateLbl.innerHTML = str1;
	}
	updateOnAjax('isProrated',ctrl,'','pck','','');
	return false;
	
	
}

function toggleAdvancePosting(ctrlTxt, ctrl, vl, str1, str0)
{
    var autoUpdatePrice = document.getElementById(ctrl);
    YAHOO.util.Dom.setStyle(ctrl, "display","none");
    var autoUpdateLbl = document.getElementById(ctrlTxt);
    console.log(str1);
    console.log(str0);
    if(vl == 0)
    {
        autoUpdateLbl.innerHTML = str0;
    }
    else
    {
        autoUpdateLbl.innerHTML = str1;
    }
}

function toggleCaptionForGdsAttribute(ctrlTxt, ctrl, vl) {
	var autoUpdatePrice = document.getElementById(ctrl);
	YAHOO.util.Dom.setStyle(ctrl, "display","none");
	var autoUpdateLbl = document.getElementById(ctrlTxt);
	
	var domArr = ["No Meal Included",
				  "Continental Breakfast",
				  "Buffet BB",
				  "Halfboard",
				  "Fullboard"	
				 ];				 
	
	if(vl == 0)
		autoUpdateLbl.innerHTML = "No meals included in this package.";
	else
		autoUpdateLbl.innerHTML = "This Package includes "+domArr[vl];	
		

	updateOnAjax('gdsMealAttribute',ctrl,'','pck','','');
	return false;	
	
}

function showHideBulk(shId,hiId)
{
	document.getElementById(shId).style.display='';
	document.getElementById(hiId).style.display='none';
}

function updateAgentComm(pkgId,validityId,from) {

	var CommType = '';
	var commition = '';	
	enadleDisableOnAjax(0);
	if(from == 'agent')
	{
	
	commition = document.getElementById('commission'+validityId).value;	
	var commitionTypeF = document.getElementById('commissionType'+validityId+'_F');
	var commitionTypeP = document.getElementById('commissionType'+validityId+'_P');	
	
	
	if(commitionTypeF.checked)	
		CommType = 	commitionTypeF.value;
	else
		CommType = 	commitionTypeP.value;	
	
	}
	else
	{
		commition = document.getElementById('commission').value;	
		var commitionTypeF = document.getElementById('commissionType_F');
		var commitionTypeP = document.getElementById('commissionType_P');	
		
		
		if(commitionTypeF.checked)
			CommType = 	commitionTypeF.value;
		else
			CommType = 	commitionTypeP.value;		
	}
	        if(document.getElementById('chkMarkUp')!= null)
        {
            var isMarkUpRate = document.getElementById('chkMarkUp').value;
            var isMarkUpRate = '&isMarkUpRate='+isMarkUpRate;
        }
        if(document.getElementById('txtFixedMargin')!=undefined)
        {
            var txtFixedMargin = document.getElementById('txtFixedMargin').value; 
            var txtFixedMargin = '&fixedMargin='+txtFixedMargin;
        }
        else
        {
            var txtFixedMargin = '';
        }
        
	var handleSuccess = function(o){ 
		if(o.responseText !== undefined){ 
			try { 
                                enadleDisableOnAjax(1);
                         if(commition > 0)
                         {
                            validityId = o.responseText;
                            window.location.href='https://25273.staygrid.com/V2/admine/packagedetails/editview/id/'+pkgId+'/wiewvalidity/'+validityId;
                         }	
                            
			}catch (x) { 
				alert("JSON Parse failed!"); 
				YAHOO.example.container.wait.hide();
				return; 
			}
		}
	
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	
	
		sUrl = 'https://25273.staygrid.com/V2/admine/packagedetails/updateagencomm';
	
		
	var postValue = 'pkgId='+pkgId+'&commission='+commition+'&commissionType='+CommType+isMarkUpRate+txtFixedMargin+'&validityId='+validityId+'&fromConsole='+from;		
	
	var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback,postValue);
       
}

function updateRelModifier(pkgId) {

    if(pkgType != 'N')
    {
        return false;
    }
	var relVal = document.getElementById('relValModfier').value;	
	var relTypeFChb = document.getElementById('relValModfierType_F');
	var relTypePChb = document.getElementById('relValModfierType_P');
	
	var relType = relTypeFChb.value;
    
	if(relTypePChb.checked)	
    {
		relType = 	relTypePChb.value;
    }
    
	var handleSuccess = function(o){ 
		if(o.responseText !== undefined){ 
			try { 
                 window.location.href='https://25273.staygrid.com/V2/admine/packagedetails/editview/id/'+pkgId+"/"+Math.random()+"/#rrTblId";	
				
			}catch (x) { 
				alert("JSON Parse failed!"); 
				YAHOO.example.container.wait.hide();
				return; 
			}
		}
	
	}
	
	var handleFailure = function(o){ 
		if(o.responseText !== undefined){ 
			return "Operation Failed";
		} 
	}
	
	var callback = { 
		success:handleSuccess, 
		failure: handleFailure, 
		argument: { foo:"foo", bar:"bar" } 
	}; 	
	
		sUrl = 'https://25273.staygrid.com/V2/admine/packagedetails/updaterelvalmodifier';
	
		
	var postValue = 'pkgId='+pkgId+'&relVal='+relVal+'&relType='+relType;		
	
	var request = YAHOO.util.Connect.asyncRequest('POST', sUrl, callback,postValue);
       
}
