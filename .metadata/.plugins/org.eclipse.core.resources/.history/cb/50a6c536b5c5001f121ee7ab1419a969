    function selectItemss(toField)
    {
        var obj = document.getElementById(toField);
        for(var i=0;i<obj.options.length;i++)
        {
            obj.options[i].selected=true;
        }
    }

    function showIncludeReservationReceive(ctrl) {
        var sty = YAHOO.util.Dom.getStyle(ctrl, "display");
        if (sty == "none") {
            YAHOO.util.Dom.setStyle(ctrl, "display", "");
        } else {
            YAHOO.util.Dom.setStyle(ctrl, "display", "none");
        }
    }

    function showIncludeReservationRate(ctrl) {
        var sty = YAHOO.util.Dom.getStyle(ctrl, "display");
        if (sty == "none") {
            YAHOO.util.Dom.setStyle(ctrl, "display", "");
        } else {
            YAHOO.util.Dom.setStyle(ctrl, "display", "none");
        }
    }

