    <script type="text/javascript" src="https://25273.staygrid.com/V2/jq/js/jquery-ui-1.11.1.min.js"></script>
    <script type="text/javascript" src="https://25273.staygrid.com/V2/jq/js/plugins/timepicker/jquery-ui-timepicker-addon.js"></script>
    
    <script type="text/javascript" src="https://25273.staygrid.com/V2/js/ckeditor/ckeditor.js"></script>
